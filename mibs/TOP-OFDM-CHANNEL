topCmtsTemplateDsOfdmChanTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF TopCmtsTemplateDsOfdmChanEntry	
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"DOCSIS 3.1 downstream ofdm channel template table."
	::={ topCmtsDocsis31ChannelObject 9 }
	
topCmtsTemplateDsOfdmChanEntry OBJECT-TYPE
	SYNTAX		TopCmtsTemplateDsOfdmChanEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"DOCSIS 3.1 downstream ofdm channel template table entry."
	INDEX {topCcmtsTemplateIndex,topCmtsTemplateDsOfdmChanChannelId}
	::= { topCmtsTemplateDsOfdmChanTable 1 }       

TopCmtsTemplateDsOfdmChanEntry ::=SEQUENCE{
           topCmtsTemplateDsOfdmChanChannelId         Unsigned32,
           topCmtsTemplateDsOfdmChanLowerBdryEncompSpectrum   Unsigned32,
           topCmtsTemplateDsOfdmChanUpperBdryEncompSpectrum   Unsigned32,
           topCmtsTemplateDsOfdmChanPlcFreq                   Unsigned32,
           topCmtsTemplateDsOfdmChanSubcarrierZeroFreq        Unsigned32,
           topCmtsTemplateDsOfdmChanSubcarrierSpacing         SubcarrierSpacingType,  
           topCmtsTemplateDsOfdmChanCyclicPrefix              DsOfdmCyclicPrefix,
           topCmtsTemplateDsOfdmChanRollOffPeriod             DsOfdmRollOffPeriod,
           topCmtsTemplateDsOfdmChanTimeInterleaverDepth      TimeInterleaverDepth,
           topCmtsTemplateDsOfdmChanNcpModulation             DsOfdmModulationType,
           topCmtsTemplateDsOfdmChanPowerTxPower              TenthdBmV,
           topCmtsTemplateDsOfdmChanAdminStatus               INTEGER,
           topCmtsTemplateDsOfdmChanDataProfileList           BITS,
           topCmtsTemplateDsOfdmChanIsPriCapableDs            TruthValue
	}  

topCmtsTemplateDsOfdmChanChannelId OBJECT-TYPE
	SYNTAX		Unsigned32(201..206)
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"DOCSIS 3.1 upstream ofdma channel ID."  
	::={ topCmtsTemplateDsOfdmChanEntry 1 }
	
topCmtsTemplateDsOfdmChanLowerBdryEncompSpectrum OBJECT-TYPE
    SYNTAX      Unsigned32(108000000..1218000000)
    UNITS       "Hz"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
         "This attribute represents the lower boundary frequency 
         of the encompassed spectrum."
    ::= { topCmtsTemplateDsOfdmChanEntry 2 }

topCmtsTemplateDsOfdmChanUpperBdryEncompSpectrum OBJECT-TYPE
    SYNTAX      Unsigned32(108000000..1218000000)
    UNITS       "Hz"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
         "This attribute represents the upper boundary frequency 
         of the encompassed spectrum."
    ::= { topCmtsTemplateDsOfdmChanEntry 3 }

topCmtsTemplateDsOfdmChanPlcFreq OBJECT-TYPE
    SYNTAX      Unsigned32(108000000..1218000000)
    UNITS       "Hz"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This is the PHY Link Channel (PLC) frequency. It is the center
         frequency of the lowest frequency subcarrier of the PLC. The 
         aim of the PLC is for the CMTS to convey to the CM the 
         physical properties of the OFDM channel."
    ::= { topCmtsTemplateDsOfdmChanEntry 4 }

topCmtsTemplateDsOfdmChanSubcarrierZeroFreq OBJECT-TYPE
    SYNTAX      Unsigned32(108000000..1218000000)
    UNITS       "Hz"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
         "This attribute specifies the center frequency of the 
          subcarrier 0 of the OFDM transmission. Note that since 
          subcarrier 0 is always excluded, it will actually be below the
          allowed downstream spectrum band. This is the frequency of 
          subcarrier X(0) in the definition of the DFT."
    ::= { topCmtsTemplateDsOfdmChanEntry 5 }
	
topCmtsTemplateDsOfdmChanSubcarrierSpacing OBJECT-TYPE
    SYNTAX      SubcarrierSpacingType
	UNITS       "kHz"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This attribute defines the subcarrier spacing associated with
         a particular FFT mode configured on the OFDM downstream 
         channel. If it is 4K mode, then the subcarrier spacing is 
         50kHz. If it is 8K mode, then the subcarrier spacing is 
         25kHz."
	DEFVAL{50}
    ::= { topCmtsTemplateDsOfdmChanEntry 6 }
	
topCmtsTemplateDsOfdmChanCyclicPrefix OBJECT-TYPE
    SYNTAX      DsOfdmCyclicPrefix
    UNITS       "number of samples"    
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Cyclic prefix enables the receiver to overcome the effects of
         inter-symbol-interference and intercarrier-interference caused
         by micro-reflections in the channel. There are five possible 
         values for the length of the CP and the choice depends on the
         delay spread of the channel - a longer delay spread requires a 
         longer cyclic prefix. The cyclic prefix (in usec) are converted 
         into samples using the sample rate of 204.8 Msamples/s and is
         an integer multiple of: 1/64 * 20 us."
	DEFVAL{256}
    ::= { topCmtsTemplateDsOfdmChanEntry 7 }
     
topCmtsTemplateDsOfdmChanRollOffPeriod OBJECT-TYPE
    SYNTAX      DsOfdmRollOffPeriod
    UNITS       "number of samples"    
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Roll off period maximizes channel capacity by sharpening the
         edges of the spectrum of the OFDM signal. For windowing 
         purposes another segment at the start of the IDFT output is
         appended to the end of the IDFT output - the roll-off postfix 
         (RP). There are five possible values for the (RP), and the 
         choice depends on the bandwidth of the channel and the number 
         of exclusion bands within the channel. A larger RP provides 
         sharper edges in the spectrum of the OFDM signal; however, 
         there is a time vs. frequency trade-off. Larger RP values 
         reduce the efficiency of transmission in the time domain, 
         but because the spectral edges are sharper, more useful 
         subcarriers appear in the frequency domain. There is an 
         optimum value for the RP that maximizes capacity for a given
         bandwidth and/or exclusion band scenario."
	DEFVAL{64}
    ::= { topCmtsTemplateDsOfdmChanEntry 8 }
     
topCmtsTemplateDsOfdmChanTimeInterleaverDepth OBJECT-TYPE    
    SYNTAX      TimeInterleaverDepth    
    UNITS       "symbols"    
    MAX-ACCESS  read-write    
    STATUS      current    
    DESCRIPTION    
        "This attribute represents the number of samples for the 
        OFDM Downstream channel. This is limited to 16 samples for 
        and 32 samples for 50 kHz and 25 kHz Subcarrier Spacing, 
        respectively."
    ::= { topCmtsTemplateDsOfdmChanEntry 9 }   
	
topCmtsTemplateDsOfdmChanNcpModulation OBJECT-TYPE    
    SYNTAX      DsOfdmModulationType
    MAX-ACCESS  read-write    
    STATUS      current    
    DESCRIPTION    
        "This attribute represents the modulation of all subcarriers in
        the NCP channel. Valid values are qpsk, qam16 and qam64."  
    DEFVAL{qam16}		
    ::= { topCmtsTemplateDsOfdmChanEntry 10 }   
	
topCmtsTemplateDsOfdmChanPowerTxPower OBJECT-TYPE
	SYNTAX		TenthdBmV
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"DOCSIS 3.1 downstream ofdm channel transmit power level applied for all active data subcarriers."  
	::={ topCmtsTemplateDsOfdmChanEntry 11 }
	
topCmtsTemplateDsOfdmChanAdminStatus OBJECT-TYPE
	SYNTAX		INTEGER {
                up(1),       -- ready to pass packets
                down(2),
                testing(3)   -- in some test mode
            }
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"DOCSIS 3.1 downstream ofdm channel administator  status."  
	DEFVAL{down}
	::={ topCmtsTemplateDsOfdmChanEntry 12 }

topCmtsTemplateDsOfdmChanDataProfileList OBJECT-TYPE
	SYNTAX		BITS{
		profileA(0),
