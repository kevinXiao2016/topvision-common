TOPVISION-OFDM-CHANNEL-MIB DEFINITIONS ::= BEGIN 

IMPORTS 
	MODULE-IDENTITY,
	OBJECT-TYPE,
	Integer32,
	TimeTicks,
	Counter64
		FROM SNMPv2-SMI
    TenthdBmV
        FROM DOCS-IF-MIB        -- RFC 4546
	TEXTUAL-CONVENTION,
	MacAddress,
	RowStatus,
	DisplayString,
	TruthValue,
	TimeStamp
		FROM SNMPv2-TC
	OBJECT-GROUP,
	MODULE-COMPLIANCE
		FROM SNMPv2-CONF
	SnmpAdminString
		FROM SNMP-FRAMEWORK-MIB   
	topCcmtsDocs31MibObject
	    FROM  TOPVISION-CCMTS-MIB
	sumaRoot,
	topvisionRoot,
	topvisionHFCCcmtsTree
		FROM SUMA-EPONEOC-EPON-MIB
	UsOfdmaModulationType,
	DsOfdmModulationType,
	SubcarrierSpacingType,
	DsOfdmCyclicPrefix,
	DsOfdmRollOffPeriod,
	TimeInterleaverDepth,
	UsOfdmaCyclicPrefix,
	UsOfdmaRollOffPeriod
		    FROM DOCS-IF31-MIB
	topCcmtsTemplateIndex
	        FROM TOPVISION-TEMPLATE-MIB
	ifIndex
			FROM IF-MIB
	AttributeMask
			FROM DOCS-IF3-MIB;
				
topCmtsDocsis31ChannelObject OBJECT IDENTIFIER ::={ topCcmtsDocs31MibObject 1 } 

