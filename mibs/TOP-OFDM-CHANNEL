topCmtsDsOfdmChanEntry OBJECT-TYPE    
    SYNTAX      TopCmtsDsOfdmChanEntry    
    MAX-ACCESS  not-accessible    
    STATUS      current    
    DESCRIPTION    
        "The conceptual row of topCmtsDsOfdmTable.    
         An instance exists for each downstream OFDM channel."    
    INDEX { 
             ifIndex 
          }
    ::= { topCmtsDsOfdmChanTable 1 }   

TopCmtsDsOfdmChanEntry ::=  SEQUENCE{    
	topCmtsDsOfdmChanChannelId                Integer32,
	topCmtsDsOfdmChanLowerBdryFreq            Unsigned32,
	topCmtsDsOfdmChanUpperBdryFreq            Unsigned32,
	topCmtsDsOfdmChanLowerBdryEncompSpectrum  Unsigned32,
	topCmtsDsOfdmChanUpperBdryEncompSpectrum  Unsigned32,
	topCmtsDsOfdmChanPlcFreq                  Unsigned32,
	topCmtsDsOfdmChanSubcarrierZeroFreq       Unsigned32,
	topCmtsDsOfdmChanFirstActiveSubcarrierNum Unsigned32,
	topCmtsDsOfdmChanLastActiveSubcarrierNum  Unsigned32,
	topCmtsDsOfdmChanNumActiveSubcarriers     Unsigned32,
	topCmtsDsOfdmChanSubcarrierSpacing        SubcarrierSpacingType,
	topCmtsDsOfdmChanLowerGuardbandWidth      Unsigned32,
	topCmtsDsOfdmChanUpperGuardbandWidth      Unsigned32,
	topCmtsDsOfdmChanCyclicPrefix             DsOfdmCyclicPrefix,
	topCmtsDsOfdmChanRollOffPeriod            DsOfdmRollOffPeriod,
	topCmtsDsOfdmChanTimeInterleaverDepth     TimeInterleaverDepth,
	topCmtsDsOfdmChanNumPilots                Unsigned32,
	topCmtsDsOfdmChanPilotScaleFactor         Unsigned32,
	topCmtsDsOfdmChanNcpModulation            DsOfdmModulationType,
	topCmtsDsOfdmChanUtilization              Unsigned32
   }    
          
topCmtsDsOfdmChanChannelId OBJECT-TYPE
     SYNTAX      Integer32 (0..255)
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION
         "The Cable Modem Termination System identification of the
          OFDM downstream channel within this particular MAC interface.
          if the interface is down, the object returns the most
          current value.  If the downstream channel ID is unknown,
          this object returns a value of 0."
     ::= { topCmtsDsOfdmChanEntry 1 }

topCmtsDsOfdmChanLowerBdryFreq OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "Hz"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
         "This attribute represents either the lower boundary frequency 
         of the lower guardband or (if no guardband is defined) the 
         lower boundary frequency of the lowest active subcarrier of the 
         OFDM downstream channel."
    ::= { topCmtsDsOfdmChanEntry 2 }

topCmtsDsOfdmChanUpperBdryFreq OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "Hz"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
         "This attribute represents either the upper boundary frequency 
         of the upper guardband or (if no guardband is defined) the 
         upper boundary frequency of the highest active subcarrier of the 
         OFDM downstream channel."
    ::= { topCmtsDsOfdmChanEntry 3 }

topCmtsDsOfdmChanLowerBdryEncompSpectrum OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "Hz"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
         "This attribute represents the lower boundary frequency 
         of the encompassed spectrum."
    ::= { topCmtsDsOfdmChanEntry 4 }

topCmtsDsOfdmChanUpperBdryEncompSpectrum OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "Hz"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
         "This attribute represents the upper boundary frequency 
         of the encompassed spectrum."
    ::= { topCmtsDsOfdmChanEntry 5 }

topCmtsDsOfdmChanPlcFreq OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "Hz"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This is the PHY Link Channel (PLC) frequency. It is the center
         frequency of the lowest frequency subcarrier of the PLC. The 
         aim of the PLC is for the CMTS to convey to the CM the 
         physical properties of the OFDM channel."
    ::= { topCmtsDsOfdmChanEntry 6 }

topCmtsDsOfdmChanSubcarrierZeroFreq OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "Hz"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
         "This attribute specifies the center frequency of the 
          subcarrier 0 of the OFDM transmission. Note that since 
          subcarrier 0 is always excluded, it will actually be below the
          allowed downstream spectrum band. This is the frequency of 
          subcarrier X(0) in the definition of the DFT."
    ::= { topCmtsDsOfdmChanEntry 7 }

topCmtsDsOfdmChanFirstActiveSubcarrierNum OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
         "This attribute corresponds to the number of the first 
          non-excluded subcarrier. The valid range is 148 to 7895."
    ::= { topCmtsDsOfdmChanEntry 8 }
     
topCmtsDsOfdmChanLastActiveSubcarrierNum OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
         "This attribute corresponds to the number of the last 
          non-excluded subcarrier. The valid range is 148 to 7895"
    ::= { topCmtsDsOfdmChanEntry 9 }
    
topCmtsDsOfdmChanNumActiveSubcarriers OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This attribute represents the number of active data 
        subcarriers within the OFDM downstream channel (i.e. this 
        exclude subcarriers for continuous pilots and the PLC). 
        For 4K FFT mode, the maximum number of subcarriers including 
        continuous pilots and the PLC cannot exceed 3800, and for 
        8K FFT mode, the maximum number of active subcarriers including 
        continuous pilots and the PLC cannot be greater than 7600. 

        However, there are a minimum of 56 continuous pilots in a 192MHz 
        channel that has no exclusions, and the size of the PLC is 
        8 subcarriers for 4K FFT mode and 16 subcarriers for 8K FFT mode. 
        Therefore the maximum value of NumActiveSubcarriers is 3736 
        (or 3800 - 56 - 8) for 4K FFT mode and 7528 (or 7600 - 56 - 16) 
        for 8K FFT mode."
    ::= { topCmtsDsOfdmChanEntry 10 }

topCmtsDsOfdmChanSubcarrierSpacing OBJECT-TYPE
    SYNTAX      SubcarrierSpacingType
    UNITS       "kHz"
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This attribute defines the subcarrier spacing associated with
         a particular FFT mode configured on the OFDM downstream 
         channel. If it is 4K mode, then the subcarrier spacing is 
         50kHz. If it is 8K mode, then the subcarrier spacing is 
         25kHz."
    ::= { topCmtsDsOfdmChanEntry 11 }

topCmtsDsOfdmChanLowerGuardbandWidth OBJECT-TYPE    
     SYNTAX      Unsigned32
     MAX-ACCESS  read-only
     STATUS      current    
     DESCRIPTION    
         "This attribute is the width in Hertz of the lower guard
         band of the OFDM channel. The valid range is 0 or 
         1MHz to 1.77GHz."    
    ::= { topCmtsDsOfdmChanEntry 12 }    

topCmtsDsOfdmChanUpperGuardbandWidth OBJECT-TYPE    
     SYNTAX      Unsigned32
     MAX-ACCESS  read-only    
     STATUS      current    
     DESCRIPTION    
         "This attribute is the width in Hertz of the upper guard 
         band of the OFDM channel. The valid range is 0 or 
         1MHz to 1.77GHz"    
    ::= { topCmtsDsOfdmChanEntry 13 }    

topCmtsDsOfdmChanCyclicPrefix OBJECT-TYPE
    SYNTAX      DsOfdmCyclicPrefix
    UNITS       "number of samples"    
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Cyclic prefix enables the receiver to overcome the effects of
         inter-symbol-interference and intercarrier-interference caused
         by micro-reflections in the channel. There are five possible 
         values for the length of the CP and the choice depends on the
         delay spread of the channel - a longer delay spread requires a 
         longer cyclic prefix. The cyclic prefix (in usec) are converted 
         into samples using the sample rate of 204.8 Msamples/s and is
         an integer multiple of: 1/64 * 20 us."
    ::= { topCmtsDsOfdmChanEntry 14 }
     
topCmtsDsOfdmChanRollOffPeriod OBJECT-TYPE
    SYNTAX      DsOfdmRollOffPeriod
    UNITS       "number of samples"    
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Roll off period maximizes channel capacity by sharpening the
         edges of the spectrum of the OFDM signal. For windowing 
         purposes another segment at the start of the IDFT output is
         appended to the end of the IDFT output - the roll-off postfix 
         (RP). There are five possible values for the (RP), and the 
         choice depends on the bandwidth of the channel and the number 
         of exclusion bands within the channel. A larger RP provides 
         sharper edges in the spectrum of the OFDM signal; however, 
         there is a time vs. frequency trade-off. Larger RP values 
         reduce the efficiency of transmission in the time domain, 
         but because the spectral edges are sharper, more useful 
         subcarriers appear in the frequency domain. There is an 
         optimum value for the RP that maximizes capacity for a given
         bandwidth and/or exclusion band scenario."
    ::= { topCmtsDsOfdmChanEntry 15 }
     
topCmtsDsOfdmChanTimeInterleaverDepth OBJECT-TYPE    
    SYNTAX      TimeInterleaverDepth    
    UNITS       "symbols"    
    MAX-ACCESS  read-write    
    STATUS      current    
    DESCRIPTION    
        "This attribute represents the number of samples for the 
        OFDM Downstream channel. This is limited to 16 samples for 
        and 32 samples for 50 kHz and 25 kHz Subcarrier Spacing, 
        respectively."
    ::= { topCmtsDsOfdmChanEntry 16 }    

topCmtsDsOfdmChanNumPilots OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This attribute is the number of continuous pilots  
        for the OFDM downstream channel."    
    ::= { topCmtsDsOfdmChanEntry 17 }    
         
topCmtsDsOfdmChanPilotScaleFactor OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "This attribute represents the scale factor for calculating
        the number of continuous pilots. "    
    ::= { topCmtsDsOfdmChanEntry 18 }    
         
topCmtsDsOfdmChanNcpModulation OBJECT-TYPE    
    SYNTAX      DsOfdmModulationType
    MAX-ACCESS  read-write    
    STATUS      current    
    DESCRIPTION    
        "This attribute represents the modulation of all subcarriers in
        the NCP channel. Valid values are qpsk, qam16 and qam64."    
    ::= { topCmtsDsOfdmChanEntry 19 }    

topCmtsDsOfdmChanUtilization OBJECT-TYPE    
    SYNTAX       Unsigned32 (0..100)
    UNITS        "percent"
    MAX-ACCESS   read-only
    STATUS       current
    DESCRIPTION
        "The utilization for this OFDM downstream channel, accurate as 
        of the most recent docsIfCmtsChannelUtilizationInterval.
        
        See Reference for specific details on this attribute."
     REFERENCE 
        "DOCSIS 3.1 CCAP Operations Support System Interface
        Specification CM-SP-CCAP OSSIv3.1-I04-15MMDD,
        OfdmChannelUtilization."
    ::= { topCmtsDsOfdmChanEntry 20 }  	

topCmtsUsOfdmaChanTable OBJECT-TYPE
     SYNTAX      SEQUENCE OF TopCmtsUsOfdmaChanEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
        "This object provides channel-level visibility into Upstream
        OFDMA channel configuration and status."
     ::= { topCmtsDocsis31ChannelObject 18 }

topCmtsUsOfdmaChanEntry OBJECT-TYPE
     SYNTAX      TopCmtsUsOfdmaChanEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
         "The conceptual row of topCmtsUsOfdmaChanTable."
     INDEX {
             ifIndex
           }
     ::= { topCmtsUsOfdmaChanTable 1 }

TopCmtsUsOfdmaChanEntry ::= SEQUENCE {
     topCmtsUsOfdmaChanTemplateIndex            Unsigned32,
     topCmtsUsOfdmaChanConfigChangeCt           Unsigned32,
     topCmtsUsOfdmaChanTargetRxPower            TenthdBmV,
     topCmtsUsOfdmaChanLowerBdryFreq            Unsigned32,
     topCmtsUsOfdmaChanUpperBdryFreq            Unsigned32,
     topCmtsUsOfdmaChanSubcarrierSpacing        SubcarrierSpacingType,
     topCmtsUsOfdmaChanCyclicPrefix             UsOfdmaCyclicPrefix,
     topCmtsUsOfdmaChanNumSymbolsPerFrame       Unsigned32,
     topCmtsUsOfdmaChanRollOffPeriod            UsOfdmaRollOffPeriod,
     topCmtsUsOfdmaChanPreEqEnable              TruthValue,
     topCmtsUsOfdmaChanFineRngGuardband         Unsigned32,
     topCmtsUsOfdmaChanFineRngNumSubcarriers    Unsigned32,
     topCmtsUsOfdmaChanFineRngPreambleLen       Unsigned32,
     topCmtsUsOfdmaChanInitRngGuardband         Unsigned32,
     topCmtsUsOfdmaChanInitRngNumSubcarriers    Unsigned32,
     topCmtsUsOfdmaChanInitRngPreambleLen       Unsigned32,
     topCmtsUsOfdmaChanProvAttribMask           AttributeMask,
     topCmtsUsOfdmaChanTxBackoffStart           Integer32,
     topCmtsUsOfdmaChanTxBackoffEnd             Integer32,
     topCmtsUsOfdmaChanRangingBackoffStart      Integer32,
     topCmtsUsOfdmaChanRangingBackoffEnd        Integer32,
     topCmtsUsOfdmaChanUtilization              Unsigned32,
     topCmtsUsOfdmaChanId                       Integer32,
	 topCmtsUsOfdmaChanSubcarrierZeroFreq		Unsigned32
     }

