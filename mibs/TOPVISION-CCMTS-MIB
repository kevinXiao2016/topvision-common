	SYNTAX		OCTET STRING (SIZE (1..32))
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"the software version number of image file."
	::={ topCcmtsCmAutoUpgradeCfgEntry 2 }
	
topCcmtsCmAutoUpgradeImageFileName OBJECT-TYPE
	SYNTAX		OCTET STRING (SIZE (1..64))
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"the name of the image file used to upgrade."
	::={ topCcmtsCmAutoUpgradeCfgEntry 3 }	

topCcmtsCmAutoUpgradeRowStatus OBJECT-TYPE
	SYNTAX		RowStatus
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"The conceptual row status of this object."
	::={ topCcmtsCmAutoUpgradeCfgEntry 4 }
	
topCcmtsCmUpgradeStatusTable OBJECT-TYPE
    SYNTAX SEQUENCE OF TopCcmtsCmUpgradeStatusEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
		" "
    ::= { topCcmtsCmUpgradeObject 3 }  
    
topCcmtsCmUpgradeStatusEntry OBJECT-TYPE
    SYNTAX TopCcmtsCmUpgradeStatusEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
		" "
	INDEX { docsIfCmtsCmStatusIndex }
    ::= { topCcmtsCmUpgradeStatusTable 1 } 
    
TopCcmtsCmUpgradeStatusEntry     ::= SEQUENCE {
	 topCcmtsCmUpgradeStatusLastSwVers OCTET STRING,
	 topCcmtsCmUpgradeStatusCurrSwVers OCTET STRING,
	 topCcmtsCmUpgradeStatusImageFileName OCTET STRING,	
     topCcmtsCmUpgradeStatusUpgradeResult INTEGER,
     topCcmtsCmUpgradeStatusUpgradeBeginTime OCTET STRING,
     topCcmtsCmUpgradeStatusUpgradeEndTime OCTET STRING		
	}
	 
topCcmtsCmUpgradeStatusLastSwVers OBJECT-TYPE
 	SYNTAX		OCTET STRING (SIZE (0..32))
 	MAX-ACCESS	read-only
	STATUS		current
    DESCRIPTION
		"the last software version of the CM." 
	::={ topCcmtsCmUpgradeStatusEntry 1 }

topCcmtsCmUpgradeStatusCurrSwVers OBJECT-TYPE
 	SYNTAX		OCTET STRING (SIZE (0..32))
 	MAX-ACCESS	read-only
	STATUS		current
    DESCRIPTION
		"the current software version of the CM." 
	::={ topCcmtsCmUpgradeStatusEntry 2 }	

topCcmtsCmUpgradeStatusImageFileName OBJECT-TYPE
	SYNTAX		OCTET STRING (SIZE (0..64))
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"the name of the used image file"
	::={ topCcmtsCmUpgradeStatusEntry 3 }  	
	
topCcmtsCmUpgradeStatusUpgradeResult OBJECT-TYPE
    SYNTAX          INTEGER  { 
			succeed(1),
			failed(2),
			upgrading(3),
			other(4),
			timeout(5)
			}
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "The result of upgrade cm operation." 
    ::= { topCcmtsCmUpgradeStatusEntry 4 } 
    
topCcmtsCmUpgradeStatusUpgradeBeginTime OBJECT-TYPE
	SYNTAX          OCTET STRING (SIZE (0..32))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "The begin time of upgrade cm operation." 
    ::= { topCcmtsCmUpgradeStatusEntry 5 }  
    
topCcmtsCmUpgradeStatusUpgradeEndTime OBJECT-TYPE
	SYNTAX          OCTET STRING (SIZE (0..32))
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "The end time of upgrade cm operation." 
    ::= { topCcmtsCmUpgradeStatusEntry 6 } 
    
