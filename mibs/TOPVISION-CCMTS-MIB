-----------------------------------------------------------------------
--topCcmtsDhcpBundleVlanTable
--added at 2013-4-27
-----------------------------------------------------------------------			
topCcmtsDhcpBundleVlanTable OBJECT-TYPE
		SYNTAX      SEQUENCE OF TopCcmtsDhcpBundleVlanEntry
   		MAX-ACCESS  not-accessible
    	STATUS      current
    	DESCRIPTION
            "This Object describes bundle vlan table.
            dhcp packet can be relayed, when a packet vlan matches the bundle vlan."
    		::= { topCcmtsDhcpObject 8 }

topCcmtsDhcpBundleVlanEntry OBJECT-TYPE
		SYNTAX      TopCcmtsDhcpBundleVlanEntry
        MAX-ACCESS  not-accessible
        STATUS      current
    	DESCRIPTION
            "This Object describes bundle vlan ."
		INDEX { topCcmtsDhcpBundleInterface}
        ::= { topCcmtsDhcpBundleVlanTable 1 }
		TopCcmtsDhcpBundleVlanEntry ::=SEQUENCE {
		topCcmtsDhcpBundleVlanMap	OCTET STRING
		}

topCcmtsDhcpBundleVlanMap OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(512))
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The Bundle vlan map. each bit stand for a vlan.
            setting bit N to 1 means adding vlan N to the bundle;
            setting bit N to 0 means deleting vlan N to the bundle,
            vlan range 1-4094, no use of bit 0 and bit 4095.
            the default setting of all bit is 1."
        ::= { topCcmtsDhcpBundleVlanEntry 1 }


