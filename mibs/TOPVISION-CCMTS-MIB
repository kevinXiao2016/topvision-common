	SYNTAX     	Unsigned32 (1..4294967295)
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"The Index of Cmts. "
	::= { topCcmtsLoadBalDynamicMoveInfoEntry 1 }

topLoadBalDynamicMoveInfoTransferTime OBJECT-TYPE
	SYNTAX     	TimeStamp
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"The system-up time that the cmts were moving the Cable Modems to the target interface"
	::= { topCcmtsLoadBalDynamicMoveInfoEntry 2 }

topLoadBalDynamicMoveInfoCmMacAddr  OBJECT-TYPE
	SYNTAX     	MacAddress
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"The MAC Address of Cable Modems"
	::= { topCcmtsLoadBalDynamicMoveInfoEntry 3 }
 
topLoadBalDynamicMoveInfoSrcUpstream  OBJECT-TYPE
	SYNTAX     	ChannelList
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"The old Upstream Channels that the Cable Modems were on" 
    DEFVAL          { ''H }	
	::= { topCcmtsLoadBalDynamicMoveInfoEntry 4 }
    	
topLoadBalDynamicMoveInfoDstUpstream  OBJECT-TYPE
	SYNTAX     	ChannelList
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"the new Upstream Channels that the Cable Modems were moved to" 
    DEFVAL          { ''H }	
	::= { topCcmtsLoadBalDynamicMoveInfoEntry 5 }
    	
topLoadBalDynamicMoveInfoSrcDownstream  OBJECT-TYPE
	SYNTAX     	ChannelList
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"The old Downstream Channels that the Cable Modems were on" 
    DEFVAL          { ''H }		
	::= { topCcmtsLoadBalDynamicMoveInfoEntry 6 }

    
topLoadBalDynamicMoveInfoDstDownstream  OBJECT-TYPE
	SYNTAX     	ChannelList
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"the new Downstream Channels that the Cable Modems were moved to " 
    DEFVAL          { ''H }	
	::= { topCcmtsLoadBalDynamicMoveInfoEntry 7 }
    
topLoadBalDynamicMoveInfoInitTech  OBJECT-TYPE
	SYNTAX     	ChChgInitTechMap
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
      "This attribute represents the initialization technique
        that the cable modem is instructed to use when
        performing multiple-channel change-over operation." 
	::= { topCcmtsLoadBalDynamicMoveInfoEntry 8 }

topLoadBalDynamicMoveInfoCmd  OBJECT-TYPE
	SYNTAX     	INTEGER {
	            	dcc(1),
	            	dbc(2)
				}
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"The load balancing MAC Management Message exchange type used by the CMTS "
	::= { topCcmtsLoadBalDynamicMoveInfoEntry 9 }

topLoadBalDynamicMoveInfoResult  OBJECT-TYPE
	SYNTAX     	INTEGER{
	    				   success(0),
	    				   pending(1),
	    				   partialService(2),
		                   cmtsAbortTimeout(3),
		                   cmtsAbortOther(4),
	                       cmtsAbortCmOffline(5),
	                       cmtsAbortIfDown(6),
	                       cmtsAbortNoCm(7),
	                       cmtsAbortNoResource(8),
	                       cmtsAbortNoChange(9),
	                       cmRejectSyntaxError(10),
	                       cmRejectBadRcc(11),
	                       cmRejectBadTcc(12),
	                       cmRejectBadClusterAssignment(13) ,
	                       cmRejectBadDSIDEncoding(14) ,
	                       cmRejectOther(15)}
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
	"This attribute represents the status of the load-balancing operation. 
	The value can't be:
	success:                      the CM has been move to the target interface success
	pending:                      the CM is in the process of  being move to the target interface
	partialService:               the CM unable to use one or more channels as instructed in the DBC-REQ
	cmtsAbortTimeout:             the CMTS Abort the transaction due to the times of retries exceed the limitation
	cmtsAbortOther:               the CMTS Abort the transaction due to Other reason
	cmtsAbortCmOffline:           the CMTS Abort the transaction due to the specified CM was Offline
	cmtsAbortIfDown:              the CMTS Abort the transaction due to the target interface has been shutdown
	cmtsAbortNoCm:                the CMTS Abort the transaction due to the specified CM wasn't exist
	cmtsAbortNoResource:          the CMTS Abort the transaction due to insufficient resource on target
	cmtsAbortNoChange:            the CMTS Abort the transaciton due to the CM already using the interface specified
	cmRejectSyntaxError:          the CM reject the dcc-req/dbc-req due to the syntax Error in req message
	cmRejectBadRCC:               the CM reject the dbc-req due to the req message contained invalied Receive Channel Configuration
	cmRejectBadTCC:               the CM reject the dbc-req due to the req message contianed invalied Transmit Channel Configuration
	cmRejectBadClusterAssignment: the CM reject the dbc-req due to the req message contained invalid Cluster Assignment
	cmRejectBadDSIDEncoding:      the CM reject the dbc-req due to the req message contained invalid DSID Encoding
	cmRejectRejectOther:          the CM reject the dbc-req due to other reason"
	::= { topCcmtsLoadBalDynamicMoveInfoEntry 10 }
	
