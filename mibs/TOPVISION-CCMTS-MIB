	SYNTAX INTEGER {
	   unknown(0),
       gbic(1),
       solder(2),
       sfp(3)
    } 
    MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"unknown(0):Unknown or unspecified
		 gbic(1):GBIC
		 solder(2):Module/connector soldered to motherboard
		 sfp(3):SFP"
	::={ topCcmtsOpticalTransceiverInfoEntry 2 }
	
