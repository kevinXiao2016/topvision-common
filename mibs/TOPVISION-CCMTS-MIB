	::={ topCcmtsCmAutoUpgradeObject 2 } 
	
topCcmtsCmAutoUpgradeCfgEntry OBJECT-TYPE
	SYNTAX		TopCcmtsCmAutoUpgradeCfgEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"the row define of the table."
	INDEX { topCcmtsCmAutoUpgradeCmModelNum }
	::= { topCcmtsCmAutoUpgradeCfgTable 1 } 

TopCcmtsCmAutoUpgradeCfgEntry ::=SEQUENCE{
	topCcmtsCmAutoUpgradeCmModelNum	OCTET STRING,
	topCcmtsCmAutoUpgradeSwVersion	OCTET STRING,
	topCcmtsCmAutoUpgradeImageFileName OCTET STRING,
	topCcmtsCmAutoUpgradeRowStatus	RowStatus
	} 
	
