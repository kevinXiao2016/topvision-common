        SYNTAX      INTEGER {
		copper(1) ,
		fiber(2) ,
		autoCopperPreferred (3) ,
		autoFiberPreferred(4),
		autoAnyPreferred(5)
		}

        MAX-ACCESS  read-write
        STATUS      deprecated            
        DESCRIPTION
            "The object is used to set main physical interface's 
	    transmision mode.For example,setting 1 means the Main
	    Interface use copper to transfer its PDUs."  
	    DEFVAL		{ 5 }
