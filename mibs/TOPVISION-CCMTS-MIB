	SYNTAX		INTEGER {deny(0),permit(1)}
	MAX-ACCESS	read-write
	STATUS		deprecated
	DESCRIPTION
		"Permit or deny the IP access the device."
	::={ topCcmtsIpFirewallEntry 4 }      
	
topCcmtsFirewallItemProtocol OBJECT-TYPE
	SYNTAX		INTEGER {telnet(1),ssh(2),snmp(3),web(4)}
	MAX-ACCESS	read-write
	STATUS		deprecated
	DESCRIPTION
		"The protocol which this firewall item will be used for."
	::={ topCcmtsIpFirewallEntry 5 }

topCcmtsFirewallItemOpcode OBJECT-TYPE
	SYNTAX		RowStatus {active(1),createAndGo(4),destroy(6)}
	MAX-ACCESS	write-only
	STATUS		deprecated
	DESCRIPTION
		"The sign of adding or deleting a record of firewall."
	::={ topCcmtsIpFirewallEntry 6 }   
	 
topCcmtsFirewallItemAddrType OBJECT-TYPE
	SYNTAX		Unsigned32 {undown(0),ipv4(1),ipv6(2)}
	MAX-ACCESS	read-write
	STATUS		deprecated
	DESCRIPTION
		"The type of ip address used."
	::={ topCcmtsIpFirewallEntry 7 }  	

topCcmtsFirewallItemIpAddrV6 OBJECT-TYPE
	SYNTAX		InetAddressIPv6(SIZE(0..16)) 
	MAX-ACCESS	read-write
	STATUS		deprecated
	DESCRIPTION
		"The value of ipv6 address."
	::={ topCcmtsIpFirewallEntry 8 }

topCcmtsFirewallItemIpPrefix OBJECT-TYPE
	SYNTAX		Unsigned32 (1..128)
	MAX-ACCESS	read-write
	STATUS		deprecated
	DESCRIPTION
		"The value of ipv6 address prefix."
	::={ topCcmtsIpFirewallEntry 9 }  
	
