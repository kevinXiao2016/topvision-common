	SYNTAX		OCTET STRING (SIZE (1..32))
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"the model number of CM."
	::={ topCcmtsCmAutoUpgradeCfgEntry 1 } 
	
