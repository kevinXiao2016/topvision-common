topCcmtsTemperatureThresholdTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF TopCcmtsTemperatureThresholdEntry	
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		""
	::={ topCcmtsSystemObject 21 }

topCcmtsTemperatureThresholdEntry OBJECT-TYPE
	SYNTAX		TopCcmtsTemperatureThresholdEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		""
	INDEX { ifIndex }
	::= { topCcmtsTemperatureThresholdTable 1 }
	
TopCcmtsTemperatureThresholdEntry ::=SEQUENCE{      
         topCcmts3218InsRedAlarmThreshold		Integer32, 
         topCcmts3218InsYellowAlarmThreshold	Integer32,    
 		 topCcmtsOtherRedAlarmThreshold			Integer32,    
         topCcmtsOtherYellowAlarmThreshold		Integer32
	}    
    	
topCcmts3218InsRedAlarmThreshold OBJECT-TYPE
    SYNTAX Integer32
    UNITS "degC"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"3218 Inside Red Alarm temperature threshold"
    ::= { topCcmtsTemperatureThresholdEntry 1 }    
    
topCcmts3218InsYellowAlarmThreshold OBJECT-TYPE
    SYNTAX Integer32
    UNITS "degC"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"3218 Inside Yellow Alarm temperature threshold"
    ::= { topCcmtsTemperatureThresholdEntry 2 }    
    
topCcmtsOtherRedAlarmThreshold OBJECT-TYPE
    SYNTAX Integer32 (-128..127)
    UNITS "degC"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"Other Red Alarm temperature threshold, value should be greater than the value of other Yellow Alarm temperature threshold."
    ::= { topCcmtsTemperatureThresholdEntry 3 }  
    
topCcmtsOtherYellowAlarmThreshold OBJECT-TYPE
    SYNTAX Integer32 (-128..127)
    UNITS "degC"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"Other Yellow Alarm temperature threshold, value should be smaller than the value of other Red Alarm temperature threshold."
    ::= { topCcmtsTemperatureThresholdEntry 4 }

	
