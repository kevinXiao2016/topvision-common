	INDEX { docsLoadBalGrpId}
	::= { topCcmtsLoadBalGrpRestrictAllCmTable 1 }   
	
TopCcmtsLoadBalGrpRestrictAllCmEntry  ::= SEQUENCE {
		topLoadBalGrpIncludeAllCm  INTEGER
	}   
	 	
topLoadBalGrpIncludeAllCm OBJECT-TYPE
	SYNTAX  INTEGER 
	{  
		Other(0),
		AddAllCm (1),
		DelAllCm (2)
	}
	MAX-ACCESS  read-write
	STATUS      current
	DESCRIPTION
	"Indicates the status of the rows in this table and can be used to set include all cm,
	the specific group must exist. 
	Set AddAllCm include all cm ,DelAllCm delete all cm from the specific group. Default is zero." 
	DEFVAL          { 0 }
	::= { topCcmtsLoadBalGrpRestrictAllCmEntry 1 }
	  
