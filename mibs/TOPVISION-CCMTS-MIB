topCcmtsObject OBJECT IDENTIFIER ::={ topCcmtsMibObject 18 }

topCcmtsSysCfgTable OBJECT-TYPE
	SYNTAX   	SEQUENCE OF  TopCcmtsSysCfgEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"config for each CMTS" 
	::={ topCcmtsObject 1 }

topCcmtsSysCfgEntry OBJECT-TYPE
	SYNTAX		TopCcmtsSysCfgEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	
		"The row of the table"
	INDEX	    { ifIndex }  
	::={ topCcmtsSysCfgTable 1 }   
	
TopCcmtsSysCfgEntry ::=SEQUENCE{
   topCcmtsSysCfgPiggyback INTEGER,
   topCcmtsSysCfgUdcEnable INTEGER,
   topCcmtsSysCfgInactiveTimeout INTEGER,
   topCcmtsSysCfgCmPollingState INTEGER
}

topCcmtsSysCfgPiggyback  OBJECT-TYPE
	SYNTAX		INTEGER {
						true(1),
						false(2)
				}
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	
		"Piggyback switch for each C-CMTS. Set true(1) to enable piggyback,false(2) to disable"   
	::={ topCcmtsSysCfgEntry 1 }
	    
topCcmtsSysCfgUdcEnable  OBJECT-TYPE
	SYNTAX		INTEGER {
						true(1),
						false(2)
				}
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	
		"UDC switch for each C-CMTS. Set true(1) to enable udc,false(2) to disable"   
	::={ topCcmtsSysCfgEntry 2 }       
  
topCcmtsSysCfgInactiveTimeout  OBJECT-TYPE
	SYNTAX		INTEGER (30..65535) 

	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	
		"InactiveTimeout for each C-CMTS. Set CM Dynamic service flow inactive timeout,The rang is [0|30~65535]"   
	::={ topCcmtsSysCfgEntry 3 }           
  
topCcmtsSysCfgCmPollingState  OBJECT-TYPE
	SYNTAX		INTEGER {
						true(1),
						false(2)
				}
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION	
		"CM polling switch for each C-CMTS. 
		Set true(1) to enable polling, false(2) to disable and then 
		docsIfCmtsCmStatusTable could got real-time data. 
		You will get real-time data if set true(1) but set topCcmtsCmPollingPeriod to zero."
	DEFVAL    { 1 }
	::={ topCcmtsSysCfgEntry 4 }

topCcmtsGlobal OBJECT IDENTIFIER ::={ topCcmtsObject 2 }  
 
topUpChannelSignalQualityRealTimeSnmpDataStatus OBJECT-TYPE
 	SYNTAX		INTEGER {
						enable(1),
						disable(2)
				}
        MAX-ACCESS  read-write
        STATUS      current   
        DESCRIPTION
            "CMTS upstream channels' signal quality Polling Status.
            enable(1): You will get real-time docsIfSignalQualityTable.
            disable(2):You can get docsIfSignalQualityTable immediately,
                       but its data is old,which is from polling."   
        DEFVAL          { enable }

        ::= { topCcmtsGlobal 1 }
topCcmtsCmPollingPeriod OBJECT-TYPE
 	SYNTAX		INTEGER
	MAX-ACCESS  read-write
	STATUS      current   
	DESCRIPTION
		"CM Polling period.
		You will get real-time docsIfCmtsCmStatusTable if set the node to zero."
	::= { topCcmtsGlobal 2 }

