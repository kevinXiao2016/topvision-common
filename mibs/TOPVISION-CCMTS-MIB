		topCcmtsCmRemoteQueryStatus			TruthValue,
		topCcmtsCmRemoteQueryCmRxPower			TenthdBmV,
		topCcmtsCmRemoteQueryCmTxPower			TenthdBmV,
		topCcmtsCmRemoteQueryCmSignalNoise		TenthdB,
		topCcmtsCmRemoteQueryCmMicroReflections		Integer32,
		topCcmtsCmRemoteQueryCmDocsBaseCapability	DocsisVersionCcmtsLocal
	 }   
	 
	 
   topCcmtsCmRemoteQueryCmIndex OBJECT-TYPE
	SYNTAX		InterfaceIndex
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Cm Index"
	::={ topCcmtsCmRemoteQueryCmEntry 1 }
  
   topCcmtsCmRemoteQueryStatus OBJECT-TYPE
	SYNTAX		TruthValue
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Specifies whether or not the Data about Cm On the Device
		is up to date"
	::={ topCcmtsCmRemoteQueryCmEntry 2 }
   
   topCcmtsCmRemoteQueryCmRxPower OBJECT-TYPE
	SYNTAX		TenthdBmV  
	UNITS		"TenthdBmV"
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The receive power as perceived for downstream data from
		this cable modem.
		If the receive power is unknown, this object returns
		a value of zero."
	::={ topCcmtsCmRemoteQueryCmEntry 3 }
           
   topCcmtsCmRemoteQueryCmTxPower OBJECT-TYPE
	SYNTAX		TenthdBmV  
	UNITS		"TenthdBmV"
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The transfer power as perceived for upstream data from
		this cable modem.
		If the transfer power is unknown, this object returns zero."
	::={ topCcmtsCmRemoteQueryCmEntry 4 }
    
   topCcmtsCmRemoteQueryCmSignalNoise OBJECT-TYPE
	SYNTAX		TenthdB     
	UNITS		"TenthdB"
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Signal/Noise ratio as perceived for downstream data from
		this cable modem.
		If the Signal/Noise is unknown, this object returns
		a value of zero."
	::={ topCcmtsCmRemoteQueryCmEntry 5 }

   topCcmtsCmRemoteQueryCmMicroReflections OBJECT-TYPE
	SYNTAX		 Integer32 (0..255)
	UNITS		"-dBc"
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Total microreflections, including in-channel response
		as perceived on this interface, measured in dBc below
		the signal level.
		This object is not assumed to return an absolutely
		accurate value, but it gives a rough indication
		of microreflections received on this interface.
		It is up to the implementer to provide information
		as accurately as possible."
	::={ topCcmtsCmRemoteQueryCmEntry 6 }
	
   topCcmtsCmRemoteQueryCmDocsBaseCapability OBJECT-TYPE
