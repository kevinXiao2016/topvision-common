topCcmtsLoginObject	OBJECT IDENTIFIER ::={ topCcmtsSystemObject 26 }  

topCcmtsLoginTryNum OBJECT-TYPE
	SYNTAX		INTEGER (1..20)
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"When the CLI login,of the same user fails for try-num times in a row, 
		this account must be locked for lock-time."
	::={ topCcmtsLoginObject 1 }    
	
topCcmtsLoginFailLockTime OBJECT-TYPE
	SYNTAX		INTEGER (5..60)
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"When the CLI login,of the same user fails for try-num times in a row, 
		this account must be locked for lock-time."
	::={ topCcmtsLoginObject 2 }

topCcmtsLoginFailLockExtTime OBJECT-TYPE
	SYNTAX		INTEGER (5..60)
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"When the CLI login,of the same user fails for try-num times in a row,
		with each try-num consecutive fails,always extend-time time is added to the lock timer."
	::={ topCcmtsLoginObject 3 }	
