    SYNTAX          INTEGER  { 
			enable(1),
			disable(2)
			}
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "This object is used to enable/disable auto upgrade CM." 
    ::= { topCcmtsCmAutoUpgradeObject 1 } 
    
topCcmtsCmAutoUpgradeCfgTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF TopCcmtsCmAutoUpgradeCfgEntry	
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
