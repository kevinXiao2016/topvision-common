    SYNTAX          DisplayString (SIZE (0..64))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "the name of the image file used to upgrade." 
    ::= { topCcmtsSingleCmUpgradeObject 2 }
 	
topCcmtsSingleCmUpgradeAction OBJECT-TYPE
    SYNTAX          INTEGER  { 
			noOperation(0),
			upgrade(1)
			}
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "This object is used to trigger an upgrade operation." 
    ::= { topCcmtsSingleCmUpgradeObject 3 }  
    
topCcmtsCmAutoUpgradeObject OBJECT IDENTIFIER ::={ topCcmtsCmUpgradeObject 2 }

