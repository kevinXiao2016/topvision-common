topCcmtsNmAccessObject	OBJECT IDENTIFIER ::={ topCcmtsMibObject 4 }

topCcmtsNmAccessTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF TopCcmtsNmAccessEntry 
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
        "This table controls access to SNMP objects by network
        management stations.  If the table is empty, access to
        SNMP objects is unrestricted."
    ::= { topCcmtsNmAccessObject 1 }

topCcmtsNmAccessEntry OBJECT-TYPE
    SYNTAX          TopCcmtsNmAccessEntry
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
        "An entry describing access to SNMP objects by a
        particular network management station.  An entry in
        this table is not readable unless the management station
        has read-write permission (either implicit if the table
        is empty, or explicit through an entry in this table).
        Entries are ordered by topCcmtsNmAccessIndex.  The first
        matching entry (e.g., matching IP address and community
        string) is used to derive access."
    INDEX           { topCcmtsNmAccessIndex } 
    ::= { topCcmtsNmAccessTable 1 }

TopCcmtsNmAccessEntry ::= SEQUENCE {
        topCcmtsNmAccessIndex       Integer32,
        topCcmtsNmAccessIp          IpAddress,
        topCcmtsNmAccessIpMask      IpAddress,
	topCcmtsNmAccessReadCommunity   OCTET STRING,
        topCcmtsNmAccessWriteCommunity   OCTET STRING,
        topCcmtsNmAccessControl     INTEGER,
        topCcmtsNmAccessStatus      RowStatus
}

topCcmtsNmAccessIndex OBJECT-TYPE
    SYNTAX          Integer32 (1..2147483647)
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
        "Index used to order the application of access
        entries." 
    ::= { topCcmtsNmAccessEntry 1 }

topCcmtsNmAccessIp OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION
        "The IP address (or subnet) of the network management
        station.  The address 0.0.0.0 is defined to mean
        any Network Management Station (NMS).  If traps are
        enabled for this entry, then the value must be the
        address of a specific device.  Implementations MAY
        recognize 255.255.255.255 as equivalent to 0.0.0.0."
    DEFVAL          { '00000000'H } 
    ::= { topCcmtsNmAccessEntry 2 }

topCcmtsNmAccessIpMask OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION
        "The IP subnet mask of the network management stations.
        If traps are enabled for this entry, then the value must
        be 0.0.0.0.  Implementations MAY recognize
        255.255.255.255 as equivalent to 0.0.0.0."
    DEFVAL          { '00000000'H } 
    ::= { topCcmtsNmAccessEntry 3 }

topCcmtsNmAccessReadCommunity OBJECT-TYPE
    SYNTAX          OCTET STRING (SIZE  (0..32))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION
        "The community string to be matched for read-access by this
        entry. "
    DEFVAL          { "public" } 
    ::= { topCcmtsNmAccessEntry 4 }

 topCcmtsNmAccessWriteCommunity OBJECT-TYPE
    SYNTAX          OCTET STRING (SIZE  (0..32))
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION
        "The community string to be matched for write-access by this
        entry. "
    DEFVAL          { "private" } 
    ::= { topCcmtsNmAccessEntry 5 }

topCcmtsNmAccessControl OBJECT-TYPE
    SYNTAX          INTEGER  {
                        none(1),
                        read(2),
                        readWrite(3),
                        roWithTraps(4),
                        rwWithTraps(5),
                        trapsOnly(6)
                    }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION
        "Specifies the type of access allowed to this NMS.
        Setting this object to none(1) causes the table entry
        to be destroyed.  Read(2) allows access by 'get' and
        'get-next' PDUs.  ReadWrite(3) allows access by 'set' as
        well.  RoWithtraps(4), rwWithTraps(5), and trapsOnly(6)
        control distribution of Trap PDUs transmitted by this
        device."
    DEFVAL          { read } 
    ::= { topCcmtsNmAccessEntry 6 }

topCcmtsNmAccessStatus OBJECT-TYPE
    SYNTAX          RowStatus
    MAX-ACCESS      read-create
    STATUS          deprecated
    DESCRIPTION
        "Controls and reflects the status of rows in this
        table.  Rows in this table may be created by either the
        create-and-go or create-and-wait paradigm.  There is no
        restriction on changing values in a row of this table
        while the row is active.

        The following objects MUST have valid values before this
        object can be set to active: topCcmtsNmAccessIp,
        topCcmtsNmAccessStatus, topCcmtsNmAccessIpMask,
        topCcmtsNmAccessReadCommunity,topCcmtsNmAccessWriteCommunity,
	topCcmtsNmAccessControl." 
    ::= { topCcmtsNmAccessEntry 7 }

topCcmtsSoftwareObject	OBJECT IDENTIFIER ::={ topCcmtsMibObject 5 }

topCcmtsSfFileObject	OBJECT IDENTIFIER ::={ topCcmtsSoftwareObject 1 } 

topCcmtsSfFileTransferType OBJECT-TYPE
    SYNTAX          INTEGER  {
			kernel(1),
			rootfs(2),
			cmcapp(3),
			config(4),
			packimage(5),
			cmimage(6),
			cmDefConfg(7),
			cm30DefConfig(8),
			cmConfig(9)
			}
    MAX-ACCESS      read-write    
    STATUS          current
    DESCRIPTION
        "This object describes software type that will be loaded to 
        this ccmts or uploaded file to a ftp server.Only config(4)
        file can be uploaded.
        Kernel(1) means Linux kernel.
        Rootfs(2) means  DOL system.
        Cmcapp(3) means  CCMTS system.
        Config(4) means config file.
		packimage(5) means a pack may be comprised of kernel,rootfs,cmcapp,etc.
		Cmimage(6) means cable modem upgrade image file.
		cmDefConfg(7) means default cable modem config file.
		cm30DefConfig(8) means default cable modem 3.0 config file.
		cmConfig(9) means cable modem config file." 
    ::= { topCcmtsSfFileObject 1 }

topCcmtsSfFileServerIpAddr OBJECT-TYPE
	SYNTAX			IpAddress
	MAX-ACCESS		read-write
	STATUS			current
    DESCRIPTION
        "The ip address of the FTP server" 
    ::= { topCcmtsSfFileObject 2 }

topCcmtsSfFileFtpUserName OBJECT-TYPE
    SYNTAX          DisplayString (SIZE (0..50))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "The username of the FTP server" 
    ::= { topCcmtsSfFileObject 3 }

topCcmtsSfFileFtpUserPassword OBJECT-TYPE
    SYNTAX          DisplayString (SIZE (0..50))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "The password of the FTP server" 
    ::= { topCcmtsSfFileObject 4 }

topCcmtsSfFileSrcPath OBJECT-TYPE
    SYNTAX          DisplayString (SIZE (0..128))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "The file path on the FTP server" 
    ::= { topCcmtsSfFileObject 5 }

topCcmtsSfFileDestPath OBJECT-TYPE
    SYNTAX          DisplayString (SIZE (0..128))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "The file path on the ccmts " 
    ::= { topCcmtsSfFileObject 6 }

topCcmtsSfFileTransferAction OBJECT-TYPE
    SYNTAX          INTEGER  { 
			noOperation(0),
			load(1),
			upload(2)
			}
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "This object is used to begin a ftp transfer between ccmts 
        and server.If set to load(1),ccmts begins to load file 
        indicated by topCcmtsSfFileTransferType and topCcmtsSfFileSrcPath 
        from ftp server indicated by topCcmtsSfFileServerIpAddr.
        If set to upload(2),ccmts begins to upload file to ftp server.
        And at this time the node topCcmtsSfFileTransferType must to 
        be set to Config(4).When read this object always return noOperation(0)." 
    ::= { topCcmtsSfFileObject 7 } 

topCcmtsSfFileTransferStatus OBJECT-TYPE
    SYNTAX          Integer32(0..100|200..209)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "This node is used to indicated the file transfer progress.
        0-100 means 0-100%.Other value means one error happens
        200 : a generic failure
        202 : download image failed
        203 : open image file failed
        204 : length of image file is invalid
        205 : check the magic of file failed
        206 : erase flash failed
        207 : write flash failed
        208 : CRC verify failed
        209 : decompress file failed" 
    ::= { topCcmtsSfFileObject 8 }    

topCcmtsSwTftpTransferObject		OBJECT IDENTIFIER ::={ topCcmtsSfFileObject 9 } 

topCcmtsSwTftpTransferFileType OBJECT-TYPE
    SYNTAX          INTEGER  {
			kernel(1),
			rootfs(2),
			cmcapp(3),
			config(4),
			packimage(5)
			}
    MAX-ACCESS      read-write    
    STATUS          current
    DESCRIPTION
        "This object describes software type that will be loaded to 
        this ccmts or uploaded file to a tftp server.Only config(4)
        file can be uploaded.
        Kernel(1) means Linux kernel.
        Rootfs(2) means  DOL system.
        Cmcapp(3) means  CCMTS system.
        Config(4) means config file.
	packimage(5) means a pack may be comprised of kernel,rootfs,cmcapp,etc." 
    ::= { topCcmtsSwTftpTransferObject 1 }

topCcmtsSwTftpTransferAction OBJECT-TYPE
    SYNTAX          INTEGER  { 
			noOperation(0),
			load(1),
			upload(2)
			}
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "This object is used to begin a tftp transfer between ccmts 
        and server.If set to load(1),ccmts begins to load file indicated 
        by topCcmtsSwFileType and docsDevSwFilename from tftp server 
        indicated by docsDevSwServerAddressType and docsDevSwServerAddress.  
        If set to upload(2),ccmts begins to upload file to tftp server.
        And at this time the node topCcmtsSwTftpTransferFileType must to 
        be set to Config(4). Different from the FTP,this object use standard 
        mib to configure the TFTP server. So before you set this node,
        you must set the standard mib docsDevSoftware must firstly. " 
    ::= { topCcmtsSwTftpTransferObject 2 } 

topCcmtsSwTftpTransferProgress OBJECT-TYPE
    SYNTAX          Integer32(0..100|200..209)
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
        "This node is used to indicated the file transfer progress.
        0-100 means 0-100%.Other value means one error happens.
        200 : a generic failure
        202 : download image failed
        203 : open image file failed
        204 : length of image file is invalid
        205 : check the magic of file failed
        206 : erase flash failed
        207 : write flash failed
        208 : CRC verify failed
        209 : decompress file failed" 
    ::= { topCcmtsSwTftpTransferObject 3 }
 
