    SYNTAX INTEGER {down(0),up(1)}
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
		"The downlink status."
    ::= { topCcmtsLinkStatusObject 1 } 
    
topCcmtsUpLinkTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF TopCcmtsUpLinkEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"This uplink table."
	::={ topCcmtsLinkStatusObject 2 }  
	
topCcmtsUpLinkEntry OBJECT-TYPE
	SYNTAX		TopCcmtsUpLinkEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"This object describes uplink information."      
	INDEX  { topCcmtsUplinkIndex }
	::={ topCcmtsUpLinkTable 1 }  
	
TopCcmtsUpLinkEntry ::=SEQUENCE {
	topCcmtsUplinkIndex		    Unsigned32,     
	topCcmtsUplinkMedia     	INTEGER,
