	SYNTAX		INTEGER(0..255)
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"GLOBAL DHCP LeaseQuery RATE."
	::={topCcmtsDhcpConfigObject 6}

topCcmtsDhcpBundleTable OBJECT-TYPE
		SYNTAX      SEQUENCE OF TopCcmtsDhcpBundleEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This Object describes bundle interface on CCMTS."
        ::= { topCcmtsDhcpObject 2 }

topCcmtsDhcpBundleEntry OBJECT-TYPE
		SYNTAX      TopCcmtsDhcpBundleEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table describes the dhcp GiAddress for DHCP Relay
	    discover packets."
		INDEX { topCcmtsDhcpBundleInterface }
        ::= { topCcmtsDhcpBundleTable 1 }

TopCcmtsDhcpBundleEntry ::= SEQUENCE {
	topCcmtsDhcpBundleInterface		OCTET STRING,
	topCcmtsDhcpBundlePolicy		INTEGER,
	topCcmtsDhcpBundleCableSourceVerify				INTEGER,	
	topCcmtsDhcpBundleVirtualPrimaryIpAddr   		IpAddress, 
	topCcmtsDhcpBundleVirtualPrimaryIpMask	        IpAddress,
	topCcmtsDhcpBundleStatus		RowStatus,
	topCcmtsDhcpBundleLeaseQueryServer			IpAddress,
	topCcmtsDhcpBundleLeaseQueryThreshold		INTEGER,
	topCcmtsDhcpBundleLeaseQueryInterval		INTEGER
}

topCcmtsDhcpBundleInterface OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(7..10))
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The Bundle interface's ID"
        ::= { topCcmtsDhcpBundleEntry 1 }

topCcmtsDhcpBundlePolicy OBJECT-TYPE
        SYNTAX      INTEGER{
		    primary(1),
		    policy(2),
		    strict(3)
		}
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The type of the device under this bundle the Giaddress used for.
	    Primary means all of
	    the devices use the same giaddress.Policy means the device use the 
	    IP address of the sub interface for its diaddress. If set strict(3), 
	    need to set topCcmtsDhcpGiAddrTable to complet Giaddr ."
        ::= { topCcmtsDhcpBundleEntry 2 }  
 
topCcmtsDhcpBundleCableSourceVerify	 OBJECT-TYPE
        SYNTAX      INTEGER{
		    on(1),
		    off(2),
		    dhcp(3)
		}
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "DHCP Relay Cable source Verify.
            on(1): means the device does verify ip and mac for packets,then drop any packets verified failed.
            off(2): means the device does not verify ip and mac for packets.
            dhcp(3): means the device does verify ip and mac for packets, but will re-verify the cpe whose packets be verified failed"
        ::= { topCcmtsDhcpBundleEntry 3 }  

topCcmtsDhcpBundleVirtualPrimaryIpAddr OBJECT-TYPE
        SYNTAX      IpAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Virtual Primary Ip of the DHCP RELAY."
        ::= { topCcmtsDhcpBundleEntry 4 }

topCcmtsDhcpBundleVirtualPrimaryIpMask OBJECT-TYPE
        SYNTAX      IpAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Virtual Primary Ip Mask of the DHCP RELAY."
        ::= { topCcmtsDhcpBundleEntry 5 }
       
topCcmtsDhcpBundleStatus  OBJECT-TYPE
	SYNTAX          RowStatus
	MAX-ACCESS      read-create
	STATUS          current
	DESCRIPTION
		"Controls and reflects the status of rows in this
		table.  Rows in this table may be created by either the
		create-and-go or create-and-wait paradigm.  There is no
		restriction on changing values in a row of this table
		while the row is active.

		The following objects MUST have valid values before this
		object can be set to active: topCcmtsDhcpBundlePolicy" 
	::= { topCcmtsDhcpBundleEntry 6 }

topCcmtsDhcpBundleLeaseQueryServer OBJECT-TYPE
        SYNTAX      IpAddress
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "DHCP Server ip for dhcp leasequery. if set 0.0.0.0, topCcmtsDhcpHelperIpAddr will be used"
        ::= { topCcmtsDhcpBundleEntry 7 }

topCcmtsDhcpBundleLeaseQueryThreshold OBJECT-TYPE
        SYNTAX      INTEGER(0..255)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "threshold for dhcp leasequery. threshold packets will be processed in interval seconds.
            see also: topCcmtsDhcpBundleLeaseQueryInterval"
        ::= { topCcmtsDhcpBundleEntry 8 }

topCcmtsDhcpBundleLeaseQueryInterval OBJECT-TYPE
        SYNTAX      INTEGER(1..10)
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "interval for dhcp leasequery.threshold packets will be processed in interval seconds.
            see also: topCcmtsDhcpBundleLeaseQueryThreshold"
        ::= { topCcmtsDhcpBundleEntry 9 }

