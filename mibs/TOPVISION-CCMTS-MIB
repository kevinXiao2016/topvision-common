--
--topCcmtsLoadBalanceObject
--
--modified by lihuanyong
--
topCcmtsLoadBalanceObject	OBJECT IDENTIFIER ::={ topCcmtsMibObject 12}

topCcmtsLoadBalCfgTable OBJECT-TYPE
    SYNTAX SEQUENCE OF TopCcmtsLoadBalCfgEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
		" Load balancing general configuration."
    ::= { topCcmtsLoadBalanceObject 1 } 
	
topCcmtsLoadBalCfgEntry OBJECT-TYPE
    SYNTAX TopCcmtsLoadBalCfgEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
		" "
	INDEX { topLoadBalConfigCmcIndex }
    ::= { topCcmtsLoadBalCfgTable 1 }

TopCcmtsLoadBalCfgEntry     ::= SEQUENCE {
	 topLoadBalConfigCmcIndex Unsigned32,
	 topLoadBalConfigEnable INTEGER,
	 topLoadBalConfigInterval Unsigned32,
	 topLoadBalConfigMaxMoves Unsigned32,
	 topLoadBalConfigMethod INTEGER,
	 topLoadBalConfigNumPeriod INTEGER,
	 topLoadBalConfigPeriod Unsigned32,
	 topLoadBalConfigTriggerThresh Unsigned32,
	 topLoadBalConfigDiffThresh Unsigned32,
	 topLoadBalConfigRangingOverrideEnable INTEGER,
     topLoadBalConfigDccInitAtdma INTEGER,
	 topLoadBalConfigDccInitScdma INTEGER,
	 topLoadBalConfigDbcInitAtdma INTEGER,
	 topLoadBalConfigDbcInitScdma INTEGER,
	 topLoadBalConfigLoadMinimum  Unsigned32, 
     topLoadBalConfigHighSysThresh  Unsigned32,
     topLoadBalConfigLowSysThresh  Unsigned32
	}
topLoadBalConfigCmcIndex OBJECT-TYPE
    SYNTAX Unsigned32 (1..4294967295)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
		"The index of CCMTS."
    ::= { topCcmtsLoadBalCfgEntry 1 }

topLoadBalConfigEnable OBJECT-TYPE
    SYNTAX INTEGER {
    	enable(1),
    	disable(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"enable(1):the load-balance operation is on at this CCMTS
		 disable(2):the load-balance operation is off at this CCMTS" 
	DEFVAL          { enable }
    ::= { topCcmtsLoadBalCfgEntry 2 } 
    
topLoadBalConfigInterval OBJECT-TYPE
    SYNTAX Unsigned32 (0..3600)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"Minimum number of seconds that must elapse before moving the same modem 
		to a different channel."
	DEFVAL          { 240 }
    ::= { topCcmtsLoadBalCfgEntry 3 } 
    
topLoadBalConfigMaxMoves OBJECT-TYPE
    SYNTAX Unsigned32 (1..1000)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"Maximum number of modems allowed to be moved per load balancer period." 
	DEFVAL          { 16 }
    ::= { topCcmtsLoadBalCfgEntry 4 }
    
topLoadBalConfigMethod OBJECT-TYPE
    SYNTAX INTEGER {
    	utilization(1),
    	service-flow(2),
		modem(3),
		UsUtilization-DsServiceFlow(4),
		UsUtilization-DsModem(5),
		UsServiceFlow-DsUtilization(6),
		UsServiceFlow-DsModem(7),
		UsModem-DsUtilization(8),
		UsModem-DsServiceFlow(9)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"The method of load-balance.
		 The method 1 is based on run-time statistics;
		 The method 2 is based on the active service-flow number for both upstream and downstream channels;
		 The method 3 is based on the number of the cable modem for both upstream and downstream channels;
		 The method 4 means run-time statistics for upstream channels and the active service-flow number for downstream channels;
		 The method 5 means run-time statistics for upstream channels and the number of the cable modem for downstream channels;
		 The method 6 means the active service-flow number for upstream channels and run-time statistics for downstream channels;
		 The method 7 means the active service-flow number for upstream channels and the number of the cable modem for downstream channels;
		 The method 8 means the number of the cable modem for upstream channels and run-time statistics for downstream channels;
		 The method 9 means the number of the cable modem for upstream channels and the active service-flow number for downstream channels.
		"  
	DEFVAL          { utilization }
    ::= { topCcmtsLoadBalCfgEntry 5 }

topLoadBalConfigNumPeriod OBJECT-TYPE
    SYNTAX INTEGER {
    	one(1),
    	two(2),
    	four(4)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"Number of periods for calculating the weighted average load. Each period 
		 gets twice the weight of the previous period. Valid values are 1, 2, or 4. 
		 Applies to Balancing Method utilization(1) only.When Balancing Method is 
		 modem(2),the value of this object is same as Balancing Method utilization(1)" 
	DEFVAL          { four }
    ::= { topCcmtsLoadBalCfgEntry 6 }
    
topLoadBalConfigPeriod OBJECT-TYPE
    SYNTAX Unsigned32 (60..3600)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"Time in seconds between load balancing checks." 
	DEFVAL          { 60 }
    ::= { topCcmtsLoadBalCfgEntry 7 }
    
topLoadBalConfigTriggerThresh OBJECT-TYPE
    SYNTAX Unsigned32 (1..100)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"A channel is considered overloaded when the load is at least this percent
		 of capacity. Below this, no attempt is made to balance its load. Applies to
		 Balancing Method utilization(1) only. When Balancing Method is modem(2), 
		 the value of this object is same as Balancing Method utilization(1)." 
	DEFVAL          { 75 }
    ::= { topCcmtsLoadBalCfgEntry 8 }
    
topLoadBalConfigDiffThresh OBJECT-TYPE
    SYNTAX Unsigned32 (1..100)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"For channels at or above the overload threshold, only consider less loaded 
		 alternate channels where the load difference is at least this percent. Applies 
		 to Balancing Method utilization(1) only. When Balancing Method is modem(2), 
		 the value of this object is same as Balancing Method utilization(1)"
	DEFVAL          { 15 }
    ::= { topCcmtsLoadBalCfgEntry 9 }
    
topLoadBalConfigRangingOverrideEnable OBJECT-TYPE
    SYNTAX INTEGER {
    	enable(1),
    	disable(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"1=enable, 2=Disable. When enabled, a modem can be directed to 
		 a different channel via RNG-RSP prior to registration" 
	DEFVAL         { enable }
    ::= { topCcmtsLoadBalCfgEntry 10 }
    
topLoadBalConfigDccInitAtdma OBJECT-TYPE
    SYNTAX INTEGER {
       broadcastInitRanging (1),
       unicastInitRanging (2),
       initRanging (3),
       direct (4)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		" broadcastInitRanging(1): Perform Broadcast initialranging on new 
		  channel before normal operation.
		  unicastInitRanging(2)  : Perform unicast ranging on new channel 
		  before normal operation.
		  initRanging(3)         : Perform either broadcast or unicast ranging 
		  on new channel before normal operation.
		  direct(4) 		: Use the new channel(s) directly without 
		  re-initializing or ranging." 
	DEFVAL          { broadcastInitRanging }
    ::= { topCcmtsLoadBalCfgEntry 11 }
    
topLoadBalConfigDccInitScdma OBJECT-TYPE
    SYNTAX INTEGER {
       broadcastInitRanging (1),
       unicastInitRanging (2),
       initRanging (3)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		" broadcastInitRanging(1): Perform Broadcast initialranging on new 
		  channel before normal operation.
		  unicastInitRanging(2)  : Perform unicast ranging on new channel 
		  before normal operation.
		  initRanging(3)         : Perform either broadcast or unicast ranging 
		  on new channel before normal operation."
	DEFVAL          { broadcastInitRanging }
    ::= { topCcmtsLoadBalCfgEntry 12 }
    
topLoadBalConfigDbcInitAtdma OBJECT-TYPE
    SYNTAX INTEGER {
       broadcastInitRanging (1),
       unicastInitRanging (2),
       initRanging (3),
       direct (4)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		" broadcastInitRanging(1): Perform Broadcast initialranging on new 
		  channel before normal operation.
		  unicastInitRanging(2)  : Perform unicast ranging on new channel 
		  before normal operation.
		  initRanging(3)         : Perform either broadcast or unicast ranging 
		  on new channel before normal operation.
		  direct(4) 		: Use the new channel(s) directly without 
		  re-initializing or ranging." 
	DEFVAL          { broadcastInitRanging }
    ::= { topCcmtsLoadBalCfgEntry 13 }
    
topLoadBalConfigDbcInitScdma OBJECT-TYPE
    SYNTAX INTEGER {
       broadcastInitRanging (1),
       unicastInitRanging (2),
       initRanging (3)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		" broadcastInitRanging(1): Perform Broadcast initialranging on new 
		  channel before normal operation.
		  unicastInitRanging(2)  : Perform unicast ranging on new channel 
		  before normal operation.
		  initRanging(3)         : Perform either broadcast or unicast ranging 
		  on new channel before normal operation."
	DEFVAL          { broadcastInitRanging }
    ::= { topCcmtsLoadBalCfgEntry 14 } 
    
topLoadBalConfigLoadMinimum OBJECT-TYPE
    SYNTAX Unsigned32 (1..100)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    		"The minimum difference value allowed between channels. 
    		 When the CM number or service flow number exceeds the value, 
     		 the CMTS will begin load balance. It's applied to the real time load balancing." 
    	DEFVAL          { 5 }
    ::= { topCcmtsLoadBalCfgEntry 15 }

topLoadBalConfigHighSysThresh OBJECT-TYPE
    SYNTAX Unsigned32 (0..100)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    		"The high system threshold value is used to configure the traffic 
    		threshold of the system. When the load balance method is utilization(1) 
    		and the system utilization exceeds the threshold for high-traffic mode, 
    		CMTS will perform load balancing based on realtime traffic." 
    	DEFVAL          { 0 }
    ::= { topCcmtsLoadBalCfgEntry 16 }
    
topLoadBalConfigLowSysThresh OBJECT-TYPE
    SYNTAX Unsigned32 (0..99)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    		"Threshold for low-traffic mode of the system in percentage." 
    	DEFVAL          { 0 }
    ::= { topCcmtsLoadBalCfgEntry 17 }

    
