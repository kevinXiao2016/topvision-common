	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"This object describes row's definition."
	INDEX { opticalTransceiverIndex }
	::={ topCcmtsOpticalTransceiverInfoTable 1 }

TopCcmtsOpticalTransceiverInfoEntry ::=SEQUENCE {
	opticalTransceiverIndex	        INTEGER, 
	opticalTransceiverSff8472Id     INTEGER,
	opticalTransceiverBitRate       INTEGER,
	opticalTransceiverWaveLen       INTEGER,  
	opticalTransceiverVendorName    DisplayString,
	opticalTransceiverVendorPn      DisplayString,
	opticalTransceiverVendorSn      DisplayString,
    opticalTransceiverDateCode      DisplayString,
	opticalTransceiverRxPower	    INTEGER,
	opticalTransceiverTxPower	    INTEGER,
	opticalTransceiverBiasCurrent   INTEGER,
	opticalTransceiverVoltage       INTEGER,
	opticalTransceiverTemperature   INTEGER
}

