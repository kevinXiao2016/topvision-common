topCcmtsAuthObject OBJECT IDENTIFIER ::= { topCcmtsMibObject 8 }
		
topCcmtsAuthTable OBJECT-TYPE
	SYNTAX SEQUENCE OF TopCcmtsAuthEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"Authorization Object."
	::= { topCcmtsAuthObject 1 }


topCcmtsAuthEntry OBJECT-TYPE
	SYNTAX TopCcmtsAuthEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION
		"An entry used to get file and authorize to ccmts.
		ifIndex is ccmts's mac domain's ifIndex."
	INDEX { ifIndex }
	::= { topCcmtsAuthTable 1 }


TopCcmtsAuthEntry ::=
	SEQUENCE { 
		topCcmtsAuthTransferProtocol
			INTEGER,
		topCcmtsAuthFileURI
			DisplayString,
		topCcmtsAuthCurrentInfo
			DisplayString,
		topCcmtsAuthApply
			INTEGER,
		topCcmtsAuthApplyError
			DisplayString,
		topCcmtsAuthApplyStatus
			INTEGER
	 }

topCcmtsAuthTransferProtocol OBJECT-TYPE
	SYNTAX INTEGER
		{
		tftp(1),
		other(2)
		}
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"Transfer protocol."
	::= { topCcmtsAuthEntry 1 }


topCcmtsAuthFileURI OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"Authorization file's URI."
	::= { topCcmtsAuthEntry 2 }

topCcmtsAuthCurrentInfo OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Current authorization Information.It contains information 
		same as topCcmtsAuthUpdateLicenseInfo descriped."
	::= { topCcmtsAuthEntry 3 }


topCcmtsAuthApply OBJECT-TYPE
	SYNTAX INTEGER
		{
		apply(1),
		noAction(2),
		other(3)
		}
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		"Apply authorization information Action. When set to apply(1),
		device will download file and authorize."
	::= { topCcmtsAuthEntry 4 } 

topCcmtsAuthApplyError OBJECT-TYPE
	SYNTAX DisplayString
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Authorization Information will be used to this ccmts.
		It contains the serial number of the License File, the
		cut off time, limited down channel number,limited up 
		channel number,etc."
	::= { topCcmtsAuthEntry 5 }



topCcmtsAuthApplyStatus OBJECT-TYPE
	SYNTAX INTEGER
		{
			init(0),
			getAUpdateCmd(1),
			tftpGetStart(2),
			tftpGetOk(3),
			tftpGetError(4),
			startSendToCmc(5),
			sendingToCmc(6),
			sendToCmcOk(7),
			sendToCmcError(8),
			completeWithError(9),
			success(10),
			operateDbFail(11),
			cmcOffline(12),
			getCmcIfindexFail(13)
		}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Last topCcmtsAuthApply was set to apply(1),device authorized status."
	::= { topCcmtsAuthEntry 6 }

topCcmtsAuthStatus OBJECT-TYPE
	SYNTAX INTEGER
		{
			inProgress(1),
			free(2)			
		}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Inprogress(1) indicates  a CCMTS is authorizing.Free(2) indicates
		the system is free that one of CCMTSs can be authorized."
	::= { topCcmtsAuthObject 2 }
