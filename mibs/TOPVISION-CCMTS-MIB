	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	
		"Control all CMs(kick or clear offline now) based on C-CMTS"
	INDEX  { ifIndex }
	::={ topCcmtsCmControlTable 1 }
TopCcmtsCmControlEntry ::=SEQUENCE{
	topCcmtsCmReset			TruthValue,
	topCcmtsCmOfflineClear		TruthValue,
	topCcmtsCmFlapClear			TruthValue
}

topCcmtsCmReset OBJECT-TYPE
	SYNTAX		TruthValue
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Set true(1) to kick all CMs belong to the C-CMTS.
		When read ,always return false(2)."
	::={ topCcmtsCmControlEntry 1 }

topCcmtsCmOfflineClear OBJECT-TYPE
	SYNTAX		TruthValue
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Set true(1) to clear all offline CMs belong to the C-CMTS.
		When read ,always return false(2)."
	::={ topCcmtsCmControlEntry 2 }    
	
topCcmtsCmFlapClear OBJECT-TYPE
	SYNTAX		TruthValue
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Set true(1) to clear all CM flaplist belong to the C-CMTS.
		When read ,always return false(2)."
	::={ topCcmtsCmControlEntry 3 }

	
