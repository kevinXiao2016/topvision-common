topCcmtsSniTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF TopCcmtsSniEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		""
	::={ topCcmtsSniObject 5 }

topCcmtsSniEntry OBJECT-TYPE
	SYNTAX		TopCcmtsSniEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION	
		""
	INDEX  { topCcmtsSniIndex }
	::={ topCcmtsSniTable 1 }
	
TopCcmtsSniEntry ::=SEQUENCE{
	topCcmtsSniIndex		INTEGER,
	topCcmtsSniInt	    	INTEGER,
	topCcmtsSniCurrentMedia	INTEGER,
	topCcmtsSniDuplexMod	INTEGER,
	topCcmtsSniClearStatistic	INTEGER, 
	topCcmtsSniDuplexSetting  INTEGER,
	topCcmtsSniSpeedSetting   INTEGER
}

topCcmtsSniIndex OBJECT-TYPE
	SYNTAX		INTEGER
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The index of the uplink interface"
	::={ topCcmtsSniEntry 1 }
 
 topCcmtsSniInt OBJECT-TYPE
    SYNTAX      INTEGER {
	copper(1) ,
	fiber(2) ,
	autoCopperPreferred (3) ,
	autoFiberPreferred(4),
	autoAnyPreferred(5),
	epon(6),
	gpon(8)
	}

       MAX-ACCESS  read-write
       STATUS      current            
       DESCRIPTION
           "The object is used to set physical interface's 
    transmision mode.For example,setting 1 means the Interface 
    use copper to transfer its PDUs. The writed value will work 
	after system reboot, and it will return current running 
	mode when reading this object."
    ::= { topCcmtsSniEntry 2 }
                                
 topCcmtsSniCurrentMedia OBJECT-TYPE
    SYNTAX      INTEGER {
	copper(1) ,
	fiber(2) ,
	epon(3),
	gpon(4)
	}

       MAX-ACCESS  read-only
       STATUS      current            
       DESCRIPTION
           "The object is used to display the current Media"  
    ::= { topCcmtsSniEntry 3 }  
                                             
 topCcmtsSniDuplexMod OBJECT-TYPE
    SYNTAX      INTEGER {
	full(1) ,
	half(2)
	}

       MAX-ACCESS  read-only
       STATUS      current            
       DESCRIPTION
           "The object is used to display the current DuplexMod"  
    ::= { topCcmtsSniEntry 4 }   
                  
 topCcmtsSniClearStatistic OBJECT-TYPE
    SYNTAX      INTEGER {
	true(1) ,
	false(2)
	}

       MAX-ACCESS  read-write
       STATUS      current            
       DESCRIPTION
           "The object is used to clear UpLink Statistic"  
    ::= { topCcmtsSniEntry 5 }
    
 topCcmtsSniDuplexSetting OBJECT-TYPE
    SYNTAX      INTEGER {
	auto(1) ,
	full(2) ,
	half(3)
	}

       MAX-ACCESS  read-write
       STATUS      current            
       DESCRIPTION
		"The object is used to show and set duplex config.
		only support set when topCcmtsSniInt is copper or fiber"  
    ::= { topCcmtsSniEntry 6 }
 topCcmtsSniSpeedSetting OBJECT-TYPE
    SYNTAX      INTEGER {
	speedAuto(1) ,
	speed10Mbps(2) ,
	speed100Mbps(3) ,
	speed1000Mbps(4)
	}

       MAX-ACCESS  read-write
       STATUS      current            
       DESCRIPTION
		"The object is used to show and set speed config
		only support set when topCcmtsSniInt is copper or fiber
		and only support set speedAuto(1) or speed1000Mbps(4) 
		when topCcmtsSniInt is fiber"  
    ::= { topCcmtsSniEntry 7 } 
    
