    DESCRIPTION
		"specific cable modem monitor global switch status 
		set enable(1) to enable disable(2) to disable"
	DEFVAL          { disable }
    ::= { topCcmtsCmMonitorSpecificGlobalCfgObject 1 } 
    
topCcmtsCmMonitorSpecificMode OBJECT-TYPE
    SYNTAX INTEGER {
    				all(1),
    				normal(2)
    				}
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"specific cable modem monitor global working mode 
		set all(1) to monitor all the modems, normal(2) to monitor modems in specified list"
	DEFVAL          { normal }
    ::= { topCcmtsCmMonitorSpecificGlobalCfgObject 2 } 

    
topCcmtsCmMonitorSpecificCmTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF TopCcmtsCmMonitorSpecificCmEntry	
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"specify a list indicates which modems will be monitored"
	::={ topCcmtsCmMonitorSpecificObject 2 } 
		
topCcmtsCmMonitorSpecificCmEntry OBJECT-TYPE
	SYNTAX		TopCcmtsCmMonitorSpecificCmEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"specify a list indicates which modems will be monitored"
	INDEX { topCcmtsSpecificCmMac }
	::= { topCcmtsCmMonitorSpecificCmTable 1 }
	
TopCcmtsCmMonitorSpecificCmEntry ::=SEQUENCE{
	topCcmtsSpecificCmMac	MacAddress,
	topCcmtsSpecificDesc	OCTET STRING,
    topCcmtsSpecificRowStatus	RowStatus
	}

topCcmtsSpecificCmMac OBJECT-TYPE
	SYNTAX		MacAddress
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"the MAC address of cable modem which will be monitored"
	::={ topCcmtsCmMonitorSpecificCmEntry 1 }
	
topCcmtsSpecificDesc OBJECT-TYPE
	SYNTAX		OCTET STRING (SIZE (1..36))
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"description of cable modem which will be monitored, optional"
	::={ topCcmtsCmMonitorSpecificCmEntry 2 }
	
topCcmtsSpecificRowStatus OBJECT-TYPE
	SYNTAX		RowStatus
	MAX-ACCESS	read-create
	STATUS		current
	DESCRIPTION
		"rowstatus"
	::={ topCcmtsCmMonitorSpecificCmEntry 3 }	


