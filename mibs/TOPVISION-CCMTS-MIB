	SYNTAX		TruthValue
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		""
	::={ topCcmtsSysControlEntry 1}

topCcmtsSysTrapControl OBJECT-TYPE 
	SYNTAX		BITS{
		cmtsInitRegReqFailTrap(0),
		cmtsInitRegRspFailTrap(1),
		cmtsInitRegAckFailTrap(2),
		cmtsDynServReqFailTrap(3),
		cmtsDynServRspFailTrap(4),
		cmtsDynServAckFailTrap(5),
		cmtsBpiInitTrap(6),
		cmtsBPKMTrap(7),
		cmtsDynamicSATrap(8),
		cmtsDCCReqFailTrap(9),
		cmtsDCCRspFailTrap(10),
		cmtsDCCAckFailTrap(11)
	}
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		""
	DEFVAL {{}}
	::={ topCcmtsSysControlEntry 2}
  
