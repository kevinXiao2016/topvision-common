	SYNTAX		INTEGER { 			  
			  on(1),
			  off(2)
		  }
	MAX-ACCESS	read-write
	STATUS		deprecated
	DESCRIPTION
		"DHCP Relay Cable source Verify."
	::={topCcmtsDhcpConfigObject 1}

--------------------------------------------------------------
-- topCcmtsDhcpAlloc 
-- modify SYNTAX at 2012-8-27
--------------------------------------------------------------
topCcmtsDhcpAlloc OBJECT-TYPE
	SYNTAX		INTEGER{
						enable(1),
						disable(2)
				} 
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"ip address dhcp-alloc."
	::={topCcmtsDhcpConfigObject 2}

topCcmtsDhcpMode OBJECT-TYPE
	SYNTAX		BITS{
					cmLow(0),   
					cmHigh(1),  
					hostLow(2),
					hostHigh(3),
					mtaLow(4), 
					mtaHigh(5),
					stbLow(6),
					stbHigh(7)
				} 
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"DHCP mode.alloc 2 bits for each device. 00 mean snooping mode,
	01 mean l2-relay mode, 10 mean l3-relay mode, 11 is invalid"
	::={topCcmtsDhcpConfigObject 3}
	
