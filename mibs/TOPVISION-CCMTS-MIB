topCcmtsRouteTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF TopCcmtsRouteEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		""
	::={ topCcmtsSystemObject 18 }

topCcmtsRouteEntry OBJECT-TYPE
	SYNTAX		TopCcmtsRouteEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		""
	INDEX { topCcmtsRouteIndex }
	::={ topCcmtsRouteTable 1 }

TopCcmtsRouteEntry ::=SEQUENCE {
	topCcmtsRouteIndex    		INTEGER,
	topCcmtsRouteDstIp          IpAddress,
	topCcmtsRouteIpMask			IpAddress,
	topCcmtsRouteNexthop        IpAddress,
	topCcmtsRoutePort           OCTET STRING,
	topCcmtsRouteStatus			RowStatus
	}

topCcmtsRouteIndex OBJECT-TYPE
	SYNTAX		INTEGER
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"The index of route table, value range is 0-77.
		The first 12(0-11) entries are read-write for static route which user can configure manually;
		The next 64(12-75) entries are readonly for direct route of VLAN interface;
		The last 2(76-77) entries are readonly for direct route of outband interface."
	::={ topCcmtsRouteEntry 1 }

topCcmtsRouteDstIp OBJECT-TYPE
	SYNTAX		IpAddress
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"The destination ip address."
	::={ topCcmtsRouteEntry 2 }
       
topCcmtsRouteIpMask OBJECT-TYPE
	SYNTAX		IpAddress	
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"The destination network mask."
	::={ topCcmtsRouteEntry 3 }

topCcmtsRouteNexthop OBJECT-TYPE
	SYNTAX		IpAddress
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"The next hop ip address"
	::={ topCcmtsRouteEntry 4 }   
	
topCcmtsRoutePort OBJECT-TYPE
	SYNTAX		OCTET STRING(SIZE(0..30)) 
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The interface name"
	::={ topCcmtsRouteEntry 5 }

topCcmtsRouteStatus OBJECT-TYPE
	SYNTAX		RowStatus
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"The status of the route"
	::={ topCcmtsRouteEntry 6 }   
	
