	topCcmtsFirewallItemIndex			Unsigned32,
	topCcmtsFirewallItemIpAddr 			IpAddress,
	topCcmtsFirewallItemIpMask			IpAddress,       
	topCcmtsFirewallItemAction 			INTEGER,
	topCcmtsFirewallItemProtocol		INTEGER,
	topCcmtsFirewallItemOpcode			RowStatus,
	topCcmtsFirewallItemAddrType		Unsigned32,
	topCcmtsFirewallItemIpAddrV6		InetAddressIPv6,
	topCcmtsFirewallItemIpPrefix		Unsigned32
}

