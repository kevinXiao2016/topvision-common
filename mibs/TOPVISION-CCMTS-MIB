TopCcmtsSysControlEntry ::=SEQUENCE{
	topCcmtsSysResetNow		TruthValue,
	topCcmtsSysTrapControl		BITS,
	topCcmtsSysNoMacBind	TruthValue,
	topCcmtsSysReconfig		TruthValue
}

