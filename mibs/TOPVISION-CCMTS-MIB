	MODULE-IDENTITY,
	IpAddress,           
	OBJECT-TYPE,  
	Unsigned32,
	Integer32,
	TimeTicks,
	Counter64
		FROM SNMPv2-SMI
	TEXTUAL-CONVENTION,
	MacAddress,
	RowStatus,
	DisplayString,
	TruthValue,
	TimeStamp
		FROM SNMPv2-TC  
	TenthdBmV,
	TenthdB,
	docsIfCmtsCmStatusIndex,
	docsIfUpChannelId,
	docsIfDownChannelId
		FROM DOCS-IF-MIB
	docsLoadBalGrpId
    	FROM DOCS-LOADBALANCING-MIB  
	InterfaceIndex
		FROM IF-MIB
    ChannelList
        FROM DOCS-IF3-MIB
        
	OBJECT-GROUP,
	MODULE-COMPLIANCE
		FROM SNMPv2-CONF
	SnmpAdminString
		FROM SNMP-FRAMEWORK-MIB
	ifIndex
		FROM IF-MIB
	docsQosIfDirection
		FROM DOCS-QOS-MIB	
	sumaRoot,
	topvisionRoot,
	topvisionHFCCcmtsTree
		FROM SUMA-EPONEOC-EPON-MIB
	InetAddressIPv6,
	InetAddressType, 
	InetAddress
        FROM INET-ADDRESS-MIB;
		
topCcmtsMIB MODULE-IDENTITY						       --The name need to be discussed.
	LAST-UPDATED "201512020000Z"
	ORGANIZATION "Topvision"
	CONTACT-INFO 
		"	Dosion Huang
			Topvision EMS group.
			Shangdi 5th Street,Haidian,Beijing,
			China,100085
			huangdongsheng@dvt.dvt.com"
	DESCRIPTION
		"The MIB module to describe the topvision CCMTS's information.			
		This MIB was firstly added."
	::={ topvisionHFCCcmtsTree 1}					
		
--Textual conventions
DocsisVersionCcmtsLocal ::= TEXTUAL-CONVENTION
	STATUS          current
	DESCRIPTION
		"Indicates the DOCSIS Radio Frequency specification being
		referenced.
		'unknown'  indicates other
		'docsis10' indicates DOCSIS 1.0.
		'docsis11' indicates DOCSIS 1.1.
		'docsis20' indicates DOCSIS 2.0.
		'docsis30' indicates DOCSIS 3.0.
		'docsis31' indicates DOCSIS 3.1."
	REFERENCE
		"DOCSIS 3.0 MAC and Upper Layer Protocols Interface
		Specification CM-SP-MULPIv3.0-I03-070223, DOCSIS
		Version section of the Common Radio Frequency
		Interface Encodings Annex."
	SYNTAX        INTEGER {
		unknown  (0),
		docsis10 (1),
		docsis11 (2),
		docsis20 (3),
		docsis30 (4),
		docsis31 (5)
	}
--Add by hzh for toploadBalMoveInfoDynamic
ChChgInitTechMap ::= TEXTUAL-CONVENTION
    STATUS          current
    DESCRIPTION
        "This data type enumerates the allowed initialization
        techniques for Dynamic Channel Change (DCC) and Dynamic 
        Bonding Change (DBC) operations. The techniques are represented
        by the 7 most significant bits (MSB). Bits 0 through 7 map to
        initialization techniques 0 through 7.  

        Each bit position represents the internal associated technique 
        as described below:

        'reinitializeMac' Reinitialize the MAC
        'broadcastInitRanging'
          Perform Broadcast initial 
          ranging on new channel before 
          normal operation
        'unicastInitRanging'
          Perform unicast ranging on new
          channel before normal operation
        'initRanging'
          Perform either broadcast or 
          unicast ranging on new channel before
          normal operation
        'direct'
          Use the new channel(s) directly 
          without re-initializing or ranging
        'probeOFDMA'
          (OFDMA channels only) Perform probing on 
          new channel before normal operation
        'unicastInitRangingOFDMA'
          (OFDMA channels only) Perform unicast 
          initial ranging (IUC3) on new channel before normal operation
        'stationRangingOFDMA'
          (OFDMA channels only) Perform station 
          ranging (IUC4) on new channel before normal operation
         Multiple bits may be set to 1 to allow the CMTS to select the 
         most suitable technique in a proprietary manner. 
         An empty value or a value with all bits in '0' means no channel
         changes allowed"

    REFERENCE
        "DOCSIS 3.1 MAC and Uper Layer Protocol Specification, 
        CM-SP-MULPIv3.1-I09-160602, Channel Assignment During 
        Registration section."
    SYNTAX          BITS {
                        reinitializeMac(0),
                        broadcastInitRanging(1),
                        unicastInitRanging(2),
                        initRanging(3),
                        direct(4),
                        probeOFDMA(5),
                        unicastInitRangingOFDMA(6),
                        stationRangingOFDMA(7)
                    }
           
