topCcmtsVlanConfigTable OBJECT-TYPE
    SYNTAX SEQUENCE OF TopCcmtsVlanConfigEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
      	"CCMTS VLAN port config table."
    ::= { topCcmtsVlanCfgObject 2 }

topCcmtsVlanConfigEntry OBJECT-TYPE
    SYNTAX TopCcmtsVlanConfigEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
      	" "
    INDEX { topCcmtsVlanIndex}
    ::= { topCcmtsVlanConfigTable 1 }

TopCcmtsVlanConfigEntry ::= SEQUENCE {
      topCcmtsVlanIndex				INTEGER,
      topCcmtsVlanName      	    OCTET STRING,
      topCcmtsTaggedPort          	OCTET STRING,
      topCcmtsUntaggedPort          OCTET STRING,   
      topCcmtsVlanFloodMode			INTEGER,
      topCcmtsVlanStatus          	RowStatus
     }

topCcmtsVlanIndex  OBJECT-TYPE
    SYNTAX 		INTEGER(0..4094)
    MAX-ACCESS 	not-accessible
    STATUS 		current
    DESCRIPTION
      	"Vlan Index"
    ::= { topCcmtsVlanConfigEntry 1 }

topCcmtsVlanName OBJECT-TYPE
    SYNTAX 		OCTET STRING (SIZE (0..128))
    MAX-ACCESS 	read-create
    STATUS 		current
    DESCRIPTION
      	"OLT VLAN name"
    ::= { topCcmtsVlanConfigEntry 2 }

topCcmtsTaggedPort OBJECT-TYPE
    SYNTAX 		OCTET STRING
    MAX-ACCESS 	read-create
    STATUS 		current
    DESCRIPTION
	      "
	      Vlan contains tagged SNI in ccmts.
	      It is presented by member port list. Each member port is identified
	      by 3-byte format. The actual length of
	      this string depends of the number of member ports in the port list.
	      The relationship is:
	      Length of the string = 3 * (NUM of member ports)
	      For example, if port 1/2 and 2/3 is the member ports,
	      then this object will be presented as:
	      01 02 00 02 03 00
	      "
    ::= { topCcmtsVlanConfigEntry 3 }

topCcmtsUntaggedPort OBJECT-TYPE
    SYNTAX 		OCTET STRING
    MAX-ACCESS 	read-create
    STATUS 		current
    DESCRIPTION
	      "Vlan contains Untagged SNI in ccmts.
	      It is presented by member port list. Each member port is identified
	      by 3-byte format. The actual length of
	      this string depends of the number of member ports in the port list.
	      The relationship is:
	      Length of the string = 3 * (NUM of member ports)
	      For example, if device 1, port 1/2 and 2/3 is the member ports,
	      then this object will be presented as:
	      01 02 00 02 03 00
	      "
    ::= { topCcmtsVlanConfigEntry 4 }
      
topCcmtsVlanFloodMode OBJECT-TYPE
    SYNTAX 		INTEGER{
    				all(1),
    				unknown(2),
    				none(3)			
    			}
    MAX-ACCESS 	read-create
    STATUS 		current
    DESCRIPTION
      	"CCMTS vlan multicast flood mode
 		all(1):all package flood
 		unkonw(2):unknown package flood
 		none(3): no package flood"
    ::= { topCcmtsVlanConfigEntry 5 }

topCcmtsVlanStatus OBJECT-TYPE
    SYNTAX 		RowStatus
    MAX-ACCESS 	read-create
    STATUS 		current
    DESCRIPTION
      	"The rowstatus of this table. It will be active(1) when you get."
    ::= { topCcmtsVlanConfigEntry 6 }

topCcmtsVifPriIpTable OBJECT-TYPE
	SYNTAX 		SEQUENCE OF TopCcmtsVifPriIpEntry
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION
		"OLT VIF primary ip table."
	::= { topCcmtsVlanCfgObject 3 }

topCcmtsVifPriIpEntry OBJECT-TYPE
	SYNTAX 		TopCcmtsVifPriIpEntry
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION
		"OLT VIF primary ip entry."
	INDEX { topCcmtsVifPriIpVlanId }
	::= { topCcmtsVifPriIpTable 1 }
			
TopCcmtsVifPriIpEntry ::=SEQUENCE { 
		topCcmtsVifPriIpVlanId		INTEGER,
		topCcmtsVifPriIpAddr		IpAddress,
		topCcmtsVifPriIpMask		IpAddress,
		topCcmtsVifPriIpStatus		RowStatus
	 }
			 
topCcmtsVifPriIpVlanId OBJECT-TYPE
	SYNTAX 		INTEGER (0..4094)
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION
