topCcmtsSyslogObject	OBJECT IDENTIFIER ::={ topCcmtsMibObject 6 }

topCcmtsSyslogServerTable OBJECT-TYPE
    SYNTAX          SEQUENCE OF TopCcmtsSyslogServerEntry 
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
        "This Table contains Syslog Server IP addresses."
    ::= { topCcmtsSyslogObject 1 }

topCcmtsSyslogServerEntry OBJECT-TYPE
    SYNTAX          TopCcmtsSyslogServerEntry 
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
        "This entry descripts a syslog server."
    INDEX	{topCcmtsSyslogServerIndex}
    ::= { topCcmtsSyslogServerTable 1 }

TopCcmtsSyslogServerEntry ::= SEQUENCE {
        topCcmtsSyslogServerIndex       Integer32,
		topCcmtsSyslogServerIpAddr      IpAddress,    
		topCcmtsSyslogServerStatus      INTEGER ,   
		topCcmtsSyslogServerIpPort  	Integer32,
		topCcmtsSyslogServerIpv6Addr    InetAddressIPv6,
		topCcmtsSyslogFacility          Integer32  
}

topCcmtsSyslogServerIndex  OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      not-accessible
    STATUS          current
    DESCRIPTION
        "Index of the Table." 
    ::= { topCcmtsSyslogServerEntry 1 }

topCcmtsSyslogServerIpAddr OBJECT-TYPE
    SYNTAX          IpAddress
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION
        "The syslog server's IP address." 
    ::= { topCcmtsSyslogServerEntry 2 }    
    
topCcmtsSyslogServerStatus OBJECT-TYPE
    SYNTAX           INTEGER  {   
    					active(1),
						create(4),
						destory(6)
                    }
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION
        "Status of the entry, when read always return active(1).
	when set to create(4),add 1 entry to the table.when set to
	drop(6),delete this entry." 
    ::= { topCcmtsSyslogServerEntry 3 }  
    
topCcmtsSyslogServerIpPort OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION
        "The syslog server's IP port." 
    ::= { topCcmtsSyslogServerEntry 4 }   

topCcmtsSyslogServerIpv6Addr OBJECT-TYPE
	SYNTAX 		InetAddressIPv6
	MAX-ACCESS 	read-create
	STATUS 		current
	DESCRIPTION
		"The syslog ip address for v6."
	::= { topCcmtsSyslogServerEntry 5 }  
    
topCcmtsSyslogFacility OBJECT-TYPE
    SYNTAX          Integer32
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
        "The syslog server's IP facility code.
        A facility code is used to specify the program type that is handling the message.
        Messages  (events) with different facilities may be handled differently. 
        The list of facilities available is defined by RFC3164.  
        Typically for CMTS, one facility code is used. 
        Default facility code is local0." 
    ::= { topCcmtsSyslogServerEntry 6 }  

