topCcmtsSysTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF TopCcmtsSysEntry	
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		""
	::={ topCcmtsSystemObject 1 }

topCcmtsSysEntry OBJECT-TYPE
	SYNTAX		TopCcmtsSysEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		""
	INDEX { ifIndex }
	::= { topCcmtsSysTable 1 }
TopCcmtsSysEntry ::=SEQUENCE{
	topCcmtsSysDescr		DisplayString,
	topCcmtsSysObjectId		OBJECT IDENTIFIER,
	topCcmtsSysUpTime		TimeTicks,
	topCcmtsSysContact		DisplayString,
	topCcmtsSysName			DisplayString,
	topCcmtsSysLocation		DisplayString,
	topCcmtsSysService		INTEGER,
	topCcmtsSysORLastChange		TimeStamp,
	topCcmtsDocsisBaseCapability	DocsisVersionCcmtsLocal,
	topCcmtsSysRAMRatio		INTEGER,                                   
	topCcmtsSysCPURatio		INTEGER,     	
	topCcmtsSysMacAddr		MacAddress,
	topCcmtsSysFlashRatio		INTEGER,
	topCcmtsSysStatus			INTEGER,
	topCcmtsSysSwVersion		DisplayString,
	topCcmtsSysSerialNumber		DisplayString,
	topCcmtsSysCmRemoteQueryStatus INTEGER,  
	topCcmtsSysHwVersion		DisplayString,
	topCcmtsSysRunTime		    INTEGER,
	topCcmtsSysDorType		    DisplayString,
	topCcmtsSysICSVersion		DisplayString,
	topCcmtsSysIPv4		        DisplayString,
	topCcmtsSysIPv6		        DisplayString,
	topCcmtsSysCmCnt		    INTEGER,
	topCcmtsSysUpstreamScQamCnt	INTEGER,
	topCcmtsSysUpstreamOfdmaCnt	INTEGER,
	topCcmtsSysDownstreamScQamCnt	INTEGER,
	topCcmtsSysDownstreamOfdmaCnt	INTEGER,
	topCcmtsSysRunningMode		INTEGER
	}

topCcmtsSysDescr OBJECT-TYPE
	SYNTAX		DisplayString (SIZE (0..255))
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"A textual description of the entity.  This value should include 
		the full name and version identification of the system's hardware 
		type, software operating-system, and networking software.  
		It is mandatory that this only contain printable ASCII characters."
	::={ topCcmtsSysEntry 1 }

topCcmtsSysObjectId OBJECT-TYPE 
	SYNTAX		OBJECT IDENTIFIER
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The vendor's authoritative identification of the network 
		management subsystem contained in the entity. 
		E type will reply 1.3.6.1.4.1.32285.11.1.1.1.6.  
		C-E type  reply 1.3.6.1.4.1.32285.11.1.1.1.3.3.
		D type will reply 1.3.6.1.4.1.32285.11.1.1.1.4.
		F type will reply 1.3.6.1.4.1.32285.11.1.1.1.7."
	::={ topCcmtsSysEntry 2}

topCcmtsSysUpTime OBJECT-TYPE 
	SYNTAX		TimeTicks
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The time (in hundredths of a second) since the network 
		management portion of the system was last re-initialized." 
	::={ topCcmtsSysEntry 3}

topCcmtsSysContact OBJECT-TYPE 
	SYNTAX		DisplayString (SIZE (0..255))
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"The CCMTS's contact."
	::={ topCcmtsSysEntry 4}

