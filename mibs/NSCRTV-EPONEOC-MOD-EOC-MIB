-- -----------------------------------------------------------------
-- The EoC CLT Device Others Group, OID: 34808.2.4.1.5
-- -----------------------------------------------------------------
modEoCCLTOtherAdminGroup OBJECT IDENTIFIER ::= { modEoCCLTAdminGroup 5 }

modEoCCLTAdminLinkTopoChange OBJECT-TYPE
	SYNTAX INTEGER {
		no-change(0),
		change(1)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		        "EoC CLT设备信道拓扑图变化识别信息。
		         no-change(0) - 没有变化,
             change(1)    - 有变化。当CLT所连接的CNU有上线或是离线时，其信道拓
             扑图发生改变，其值变为1.直到EMS服务器对其处理后，其
             值才恢复为0；
					   此对象值由0到1的变化，往往伴随着一个设备上线离线的
             陷阱产生.
             针对主机框"
	DEFVAL { 0 }
	::= { modEoCCLTOtherAdminGroup 1 }

modEoCCLTAdminAppropriativeOID OBJECT-TYPE
	SYNTAX OBJECT IDENTIFIER
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		        "此对象为一个OID指针，用于指向被管设备专用MIB,例如：企业专用MIB.
		        针对主机框，需要加以详细描述"
	::= { modEoCCLTOtherAdminGroup 2 }

modEoCCLTTelnetTimeout OBJECT-TYPE
	SYNTAX INTEGER
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		        "telnet 服务器超时时间设置"
	::= { modEoCCLTOtherAdminGroup 3 }

