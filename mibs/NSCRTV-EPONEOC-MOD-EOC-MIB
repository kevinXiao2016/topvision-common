		snmpv1(0),
		snmpv2c(1)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		        "SNMP版本号:
		         snmpv1(0)  - SNMPv1
		         snmpv2c(1) - SNMPv2c
		         针对主机框"
