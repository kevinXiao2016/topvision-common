	::= { modEoCSoftwareUpgradeEntry 5 }

modEoCSoftwareUpgradeProceed OBJECT-TYPE
	SYNTAX INTEGER	{
		download(1),
		upgrade(2),
		backup(201),
		restore(202)
	}
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
						"设备软件升级操作：
						 download(1) - 从软件升级服务器上下载固件到EoC CLT设备.
						 upgrade(2)  - 升级EoC CLT或CNU设备固件.                                        .
						 backup(201) - 备份CBAT的配置文件.
						 restore(202)- 导入CBAT的配置文件."
	::= { modEoCSoftwareUpgradeEntry 6 }

		
modEoCSoftwareUpgradeResult OBJECT-TYPE
	SYNTAX INTEGER {
		none(0),
		downloading(1),
		downloadfailed(2),
		downloadsuccess(3),
		upgrading(4),
		upgradefailed(5),
		upgradesuccess(6), 
		downloading2(201),
		downloadfailed2(202),
		invalidfile(203),
		restoresuccess(204),
		uploading(205),
		backupfailed(206),
		backupsuccess(207)		
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
						"软件升级状态:
 		 					none(0) - 无操作.
		 					downloading(1) - 固件下载中.
		 					downloadfailed(2) - 固件下载失败.
		 					downloadsuccess(3)- 固件下载成功.
		 					upgrading(4) - 固件软件升级中.
		 					upgradefailed(5) - 固件软件升级失败.
		 					upgradesuccess(6) - 固件软件升级成功.
		 					downloading2(201) - 配置文件下载中.
		 					downloadfailed2(202) -配置文件下载失败.
		 					invalidfile(203)  - 配置文件非法.
		 					restoresuccess(204)- 导入配置文件失败.
		 					uploading(205)    - 上传配置文件中.
		 					backupfailed(206) - 备份配置文件失败.
		 					backupsuccess(207) - 备份配置文件成功."    
		 
	DEFVAL { 0 }
	::= { modEoCSoftwareUpgradeEntry 7 }

