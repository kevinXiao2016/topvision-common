-- -----------------------------------------------------------------
-- The EoC CLT Device STatus Group, OID: 34808.2.4.1.3
-- -----------------------------------------------------------------
modEoCCLTDevStatusGroup OBJECT IDENTIFIER ::= { modEoCCLTAdminGroup 3 }

modEoCCLTAdminAlarmDetectionControl OBJECT-TYPE
	SYNTAX INTEGER {
		detectionDisabled(1),
		detectionEnabled(2),
		detectionEnabledAndRegenerate(3)
	}
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		        "EoC CLT设备告警检测开关，用于控制EOC CLT设备模拟和离散属性表中的告
             警检测，该值应保存在非易失性存储器中。          
		         detectionDisabled(1) - 禁用EoC CLT设备的告警检测。
		         detectionEnabled(2)  - 允许EoC CLT设备的告警检测。
		         detectionEnabledAndRegenerate(3) - 清除EoC CLT设备的告警记录表
             alarmLogTable和当前告警表 currentAlarmTable中的所有告警信息并
             且允许告警检测。模拟属性表和离散属性表中的所有告警属性将恢复
             到缺省的正常值，但仍处于告警状态的属性将不会恢复到缺省的正常值。
             针对主机框"   
	DEFVAL { 2 }
	::= { modEoCCLTDevStatusGroup 1 }

modEoCCLTAdminStatus OBJECT-TYPE
	SYNTAX INTEGER
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		        "EoC CLT设备登记状态，采用位来表示
		         Bit 0: EOC CLT设备是否有消息要发送,1:有消息待发,0:无消息发送.消息
             发送完毕后需清0。          
		         Bit 1: 1: 有严重告警产生, 0: 无严重告警。
		         Bit 2: 1: 有非严重告警产生, 0: 无严重告警。
		         其他位：保留。
		         针对主机框"
	::= { modEoCCLTDevStatusGroup 2 }

modEoCCLTAdminInternalTemperature OBJECT-TYPE
	SYNTAX INTEGER (-128..127)      
	UNITS "C"
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		        "EoC CLT设备的机内温度，单位为摄氏度。
		        针对主机框"
	::= { modEoCCLTDevStatusGroup 3 }

modEoCCLTAdminTamperStatus OBJECT-TYPE
	SYNTAX INTEGER {
		intact(1),
		compromised(2)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		        "EOC CLT设备的安全开关(如盖子是否打开)状态，此对象要求在离散属性表
             中登记一个条目。
		         intact(1) - EoC CLT设备正常
		         compromised(2) - EoC CLT设备安全开关打开。
		         针对主机框"
	::= { modEoCCLTDevStatusGroup 4 }

modEoCCLTAdminDateAndTime OBJECT-TYPE
	SYNTAX DateAndTime
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		        "EOC CLT设备的当前时间。
		         时间格式：年-月-日，时:分:秒，时区。
		         例如: 2009-7-15, 13:20:12.3, +8:0
		         对于本地时间，时区信息可以忽略，
		         针对主机框"
	::= { modEoCCLTDevStatusGroup 5 }

modEoCCLTAdminReset OBJECT-TYPE
	SYNTAX INTEGER { reset(1) }
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		        "复位EoC CLT设备：
		         reset(1) - 复位EOC设备。
		         针对主机框"
	::= { modEoCCLTDevStatusGroup 6 }

modEoCCLTAdminResetCause OBJECT-TYPE
	SYNTAX INTEGER {
		other(1),
		powerup(2),
		command(3),
		watchdog(4),
		craft(5)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		        "EoC CLT设备最近一次复位的原因:
		         other(1)    - 其他原因,
		         powerup(2)  - 上电复位,
		         command(3)  - 软件命令复位,
		         watchdog(4) - 看门狗复位,
		         craft(5)    - EoC CLT设备本地接口复位.
		         针对主机框"
	::= { modEoCCLTDevStatusGroup 7 }

modEoCCLTAdminConfigurationSaving OBJECT-TYPE
	SYNTAX INTEGER { save(1) }
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		        "指示EoC CLT设备保存当前配置至非易失性存储器。
		        针对主机框"
	::= { modEoCCLTDevStatusGroup 8 }

modEoCCLTAdminConfigurationResult OBJECT-TYPE
	SYNTAX INTEGER {
		success(0),
		running(1),
		failed(2)
	}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		        "保存EoC CLT设备当前配置的结果:
		         successful(0) - 保存当前配成功,
		         running(1)    - 正在保存当前配置,
		         failed(2)     - 保存当前配置失败.
		         针对主机框"
	::= { modEoCCLTDevStatusGroup 9 }

modEoCCLTAdminCPULoad OBJECT-TYPE
	SYNTAX INTEGER
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		        "指示EoC设备CPU负载"
	::= { modEoCCLTDevStatusGroup 100 }

modEoCCLTAdminRAMLoad OBJECT-TYPE
	SYNTAX INTEGER
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		        "指示EoC设备内存负载"
	::= { modEoCCLTDevStatusGroup 101 }

modEoCCLTAdminUPLinkRate OBJECT-TYPE
	SYNTAX INTEGER
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		        "指示EoC CLT设备上行端口速率"
	::= { modEoCCLTDevStatusGroup 102 }

