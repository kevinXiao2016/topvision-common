NSCRTV-EPONEOC-MOD-EOC-MIB DEFINITIONS ::= BEGIN

IMPORTS
	enterprises
         FROM RFC1155-SMI
	DisplayString			
		FROM RFC1213-MIB
	TimeTicks, IpAddress, Counter64, OBJECT-TYPE, MODULE-IDENTITY,OBJECT-IDENTITY,NOTIFICATION-TYPE				
		FROM SNMPv2-SMI			
	DateAndTime, MacAddress, RowStatus
		FROM SNMPv2-TC                
	TruthValue, TimeInterval
		FROM SNMPv2-TC
    modEoCTree 
        FROM NSCRTV-ROOT
;

modEoCCLTAdminGroup OBJECT IDENTIFIER ::= { modEoCTree 1 }  

modEoCCLTDevInfoGroup OBJECT IDENTIFIER ::= { modEoCCLTAdminGroup 1 }

