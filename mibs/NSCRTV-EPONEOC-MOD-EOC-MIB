modEoCTrapMaxTimes 				OBJECT-TYPE
    SYNTAX 					INTEGER (1..255)
    MAX-ACCESS 				read-write
    STATUS 					optional
    DESCRIPTION 			
             "每条TRAP发送的最大次数。"
    ::= {  modEoCTrapGroup 1  }

modEoCTrapMinInterval 			OBJECT-TYPE
    SYNTAX 					TimeInterval 
    MAX-ACCESS 				read-write
    STATUS 					optional
    DESCRIPTION 			
              "发送TRAP的最小时间间隔，单位为百分之一秒。"
   ::= {  modEoCTrapGroup 2  }

modEoCTrapMaxInterval 			OBJECT-TYPE
    SYNTAX 					TimeInterval 
    MAX-ACCESS				read-write
    STATUS 					optional
    DESCRIPTION 			
              "发送TRAP的最大时间间隔，单位为百分之一秒。"
   ::= { 	modEoCTrapGroup 3  }

modEoCTrapIP					OBJECT-TYPE 		
	SYNTAX					IpAddress 		
	MAX-ACCESS				read-write 		
	STATUS					optional  		
	DESCRIPTION				
						" Trap目的主机IP地址" 		
	::=  {  modEoCTrapGroup 4 } 
modEoCTrapCommunity 			OBJECT-TYPE
     SYNTAX 				DisplayString
     MAX-ACCESS				read-write
     STATUS				    optional
     DESCRIPTION     		
     					"发送TRAP的共同体串。"
    ::= {  modEoCTrapGroup 5  }

modEoCTrapStatus 				OBJECT-TYPE
     SYNTAX 				TruthValue
     MAX-ACCESS				read-write
     STATUS 				optional
     DESCRIPTION    		
     					"TRAP状态:“1”使用TRAP；“2”不使用TRAP。"
    ::= {  modEoCTrapGroup 6 }

