modEoCIPAddress 				OBJECT-TYPE
    SYNTAX 					IpAddress
    MAX-ACCESS 			read-write
    STATUS 					current
    DESCRIPTION 			
             "CBAT IP 地址"
    ::= {  modEoCCBATGroup 10  }

modEoCNetmask 				OBJECT-TYPE
    SYNTAX 					IpAddress
    MAX-ACCESS 			read-write
    STATUS 					current
    DESCRIPTION 			
             "CBAT 网络掩码"
    ::= {  modEoCCBATGroup 11  }

modEoCGateway 				OBJECT-TYPE
    SYNTAX 					IpAddress
    MAX-ACCESS 			read-write
    STATUS 					current
    DESCRIPTION 			
             "CBAT 网关地址"
    ::= {  modEoCCBATGroup 12  }

modEoCMacAddress 				OBJECT-TYPE
    SYNTAX 					MacAddress
    MAX-ACCESS 			read-only
    STATUS 					current
    DESCRIPTION 			
             "CBAT MAC 地址"
    ::= {  modEoCCBATGroup 13  }

modPLCTech 				OBJECT-TYPE
    SYNTAX 					INTEGER {
		moca(1),
		homePlugBPL(2),
		homePlugAV(3),
		homePNA(4),
		wiFi(5),
		others(6)
	}
    MAX-ACCESS 			read-only
    STATUS 					current
    DESCRIPTION 			
             "CBAT 采取的技术"
    ::= {  modEoCCBATGroup 14  }

modEoCVendorInfo 				OBJECT-TYPE
    SYNTAX 					OCTET STRING(SIZE(0..255))
    MAX-ACCESS 			read-only
    STATUS 					current
    DESCRIPTION 			
             "CBAT 制造商信息"
    ::= {  modEoCCBATGroup 15  }

modEoCModel 				OBJECT-TYPE
    SYNTAX 					OCTET STRING(SIZE(0..255))
    MAX-ACCESS 			read-only
    STATUS 					current
    DESCRIPTION 			
             "CBAT 型号"
    ::= {  modEoCCBATGroup 16  }
    
modEoCEMSIPAddress OBJECT-TYPE
	SYNTAX IpAddress
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
		        "网管服务器(EMS)IP地址。"
	::= { modEoCCBATGroup 17 }

modEocStatus OBJECT-TYPE
	SYNTAX INTEGER {
		NOACTIVE(0),
		ACTIVE(1)
	}
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
	          "配置信息实际提交"
  ::= { modEoCCBATGroup 18 }
      
                                                                
END