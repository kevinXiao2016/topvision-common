--*
--* EoC CLT 射频信道管理组
--*
modEoCCLTRFGroup 	OBJECT IDENTIFIER ::= { modEoCTree 2 }

modEoCCLTRFTable 	OBJECT-TYPE
		SYNTAX 			SEQUENCE OF ModEoCCLTRFEntry
		MAX-ACCESS 		not-accessible
		STATUS 			mandatory
		DESCRIPTION		
		          "CLT射频信道管理表单。"
		::= { modEoCCLTRFGroup 1 }

modEoCCLTRFEntry 	OBJECT-TYPE
		SYNTAX 			ModEoCCLTRFEntry
		MAX-ACCESS 		not-accessible
		STATUS 			mandatory
		DESCRIPTION		
		          "CLT射频信道管理表目。"
		INDEX { modEoCCLTRFIndex }
		::= { modEoCCLTRFTable 1 }

ModEoCCLTRFEntry ::=
			SEQUENCE { 
				modEoCCLTRFIndex				
				INTEGER,
				modEoCCLTRFLinkMaximum			
				INTEGER,
				modEoCCLTRFCLTOutputLevel		
				INTEGER,
				modEoCCLTRFDownstreamStartFreq	
				INTEGER,
				modEoCCLTRFDownstreamStopFreq	
				INTEGER,
				modEoCCLTRFUpstreamStartFreq	
				INTEGER,
				modEoCCLTRFUpstreamStopFreq		
				INTEGER
			 }

modEoCCLTRFIndex		OBJECT-TYPE
		SYNTAX 			INTEGER
		MAX-ACCESS	 	read-only
		STATUS 			mandatory
		DESCRIPTION		
		          "CLT的序号索引
		           针对头端，1-2-3-4"
		::= { modEoCCLTRFEntry 1 }

modEoCCLTRFLinkMaximum 	OBJECT-TYPE
		SYNTAX 			INTEGER
		MAX-ACCESS 		read-only
		STATUS 			mandatory
		DESCRIPTION		
		          "CLT射频信道最大链路数。"
		::= { modEoCCLTRFEntry 2 }

modEoCCLTRFCLTOutputLevel		OBJECT-TYPE
		SYNTAX 			INTEGER
		MAX-ACCESS 		read-write
		STATUS 			optional
		DESCRIPTION		
		          "CLT射频输出功率电平，单位为dBuV。。"
		::= { modEoCCLTRFEntry 3 }

modEoCCLTRFDownstreamStartFreq	OBJECT-TYPE
		SYNTAX 			INTEGER
		MAX-ACCESS 		read-write
		STATUS 			mandatory
		DESCRIPTION		
		          "CLT射频下行信道起始频率，单位为Hz。"
		::= { modEoCCLTRFEntry 4 }

modEoCCLTRFDownstreamStopFreq 	OBJECT-TYPE
		SYNTAX 			INTEGER
		MAX-ACCESS 		read-write
		STATUS 			mandatory
		DESCRIPTION		
		          "CLT射频下行信道终止频率，单位为Hz。"
		::= { modEoCCLTRFEntry 5 }

modEoCCLTRFUpstreamStartFreq OBJECT-TYPE
		SYNTAX		 	INTEGER 
		MAX-ACCESS 		read-write
		STATUS 			mandatory
		DESCRIPTION		
		          "CLT射频上行信道起始频率，单位为Hz。"
		::= { modEoCCLTRFEntry 6 }

modEoCCLTRFUpstreamStopFreq OBJECT-TYPE
		SYNTAX 			INTEGER
		MAX-ACCESS 		read-write
		STATUS 			mandatory
		DESCRIPTION		
		          "CLT射频上行信道终止频率，单位为Hz。"
		::= { modEoCCLTRFEntry 7 }

--*
--* EoC CLT MAC层管理组
--*

modEoCCLTMACGroup 		OBJECT IDENTIFIER ::= { modEoCTree 3 }

modEoCCLTMACVlanTable	OBJECT-TYPE 		    
	SYNTAX			SEQUENCE  OF  ModEoCCLTMACVlanEntry  		    
	MAX-ACCESS		not-accessible 		    
	STATUS			mandatory  		    
	DESCRIPTION		
						"CLT MAC层Vlan表单" 		    
	::=  { modEoCCLTMACGroup 1  } 

modEoCCLTMACVlanEntry	OBJECT-TYPE 		    
	SYNTAX			ModEoCCLTMACVlanEntry
	MAX-ACCESS		not-accessible 		    
	STATUS			mandatory  		    
	DESCRIPTION		
						"CLT MAC层Vlan表目" 		    
	INDEX 	{ modEoCCLTMACVlanIndex  }
    ::=  {  modEoCCLTMACVlanTable 1  }

ModEoCCLTMACVlanEntry::=  SEQUENCE { 		    
	modEoCCLTMACVlanIndex    INTEGER, 		    
	modEoCCLTMACVlanEN 		 TruthValue, 		    
	modEoCCLTMACAdminVlanID  INTEGER
} 
modEoCCLTMACVlanIndex	OBJECT-TYPE 		    
	SYNTAX			INTEGER  		    
	MAX-ACCESS		read-only 		    
	STATUS			mandatory  		    
	DESCRIPTION		
						"CLT序号索引
						针对头端 1-2-3-4"             
::=  {  modEoCCLTMACVlanEntry 1  } 

modEoCCLTMACVlanEN	OBJECT-TYPE 		    
	SYNTAX			TruthValue  		    
	MAX-ACCESS		read-write 		    
	STATUS			mandatory  		    
	DESCRIPTION		
						"CLT的Vlan使能，“1”表示启用，“0”表示不启用" 		    
	::=  {  modEoCCLTMACVlanEntry  2  } 

modEoCCLTMACAdminVlanID	OBJECT-TYPE
		SYNTAX			INTEGER	  ( 0..4095 )
    MAX-ACCESS		read-write
		STATUS			mandatory
		DESCRIPTION		
							"CLT管理VLAN ID号"
		::={  modEoCCLTMACVlanEntry 3  }

modEoCCLTMACBroadcastTable	OBJECT-TYPE 		    
	SYNTAX			SEQUENCE  OF  ModEoCCLTMACBroadcastEntry  		    
	MAX-ACCESS		not-accessible 		    
	STATUS			mandatory  		    
	DESCRIPTION		
						"CLT MAC层广播风暴管理表单" 		    
	::=  {  modEoCCLTMACGroup 2  } 

modEoCCLTMACBroadcastEntry	OBJECT-TYPE 		    
	SYNTAX			ModEoCCLTMACBroadcastEntry 		    
	MAX-ACCESS		not-accessible 		    
	STATUS			mandatory   		    
	DESCRIPTION		
						"CLT MAC层广播风暴管理表目" 		    
	INDEX    {  modEoCCLTMACBroadcastIndex  }
  ::=  {  modEoCCLTMACBroadcastTable 1  }

ModEoCCLTMACBroadcastEntry::=  SEQUENCE { 		    
	modEoCCLTMACBroadcastIndex			
	INTEGER, 		    
	modEoCCLTMACBroadcastRestrictionEN	
	TruthValue, 		    
	modEoCCLTMACBroadcastRestriction 	
	INTEGER,
	modEoCCLTMACUnknowRestrictionEN		
	TruthValue,
	modEoCCLTMACUnknowRestriction		
	INTEGER
} 
modEoCCLTMACBroadcastIndex	OBJECT-TYPE 		    
	SYNTAX			INTEGER  		    
	MAX-ACCESS		read-only 		    
	STATUS			mandatory  		    
	DESCRIPTION		
						"CLT 的序号索引
						针对头端 1-2-3-4" 		    
	::=  {  modEoCCLTMACBroadcastEntry 1  } 

modEoCCLTMACBroadcastRestrictionEN	OBJECT-TYPE 		    
	SYNTAX			TruthValue  		    
	MAX-ACCESS		read-write 		    
	STATUS			mandatory  		    
	DESCRIPTION		
						"CLT 广播包抑制使能。“1”表示处理，“0”表示不处理" 		    
	::=  {  modEoCCLTMACBroadcastEntry 2  } 

modEoCCLTMACBroadcastRestriction		OBJECT-TYPE 		    
	SYNTAX			INTEGER 
	MAX-ACCESS		read-write 		    
	STATUS			mandatory  		    
	DESCRIPTION		
						"CLT 广播包抑制值，即允许通过的广播包数量"
  ::=  {  modEoCCLTMACBroadcastEntry 3  } 
        
modEoCCLTMACUnknowRestrictionEN		OBJECT-TYPE 		    
	SYNTAX			TruthValue  		    
	MAX-ACCESS		read-write 		    
	STATUS			mandatory  		    
	DESCRIPTION		
						"CLT 未知包抑制使能。“1”表示处理，“0”表示不处理" 		    
	::=  {  modEoCCLTMACBroadcastEntry 4  } 

modEoCCLTMACUnknowRestriction		OBJECT-TYPE 		   
 SYNTAX			INTEGER 
 MAX-ACCESS		read-write 		    
 STATUS			mandatory  		    
 DESCRIPTION		
