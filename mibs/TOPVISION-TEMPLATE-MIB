	topCcmtsTemplateCmRemoteQueryStatus					INTEGER,
	topCcmtsTemplateCmRemoteQueryCommunity				DisplayString,
	topCcmtsTemplateCmRemoteQueryInterval				INTEGER,
	topCcmtsTemplateCmRemoteQuerySrcIP					IpAddress  
}

topCcmtsTemplateCmRemoteQueryStatus OBJECT-TYPE
	SYNTAX		INTEGER{   
					enable(1),
					disable(2)
				}
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"enable(1):the remotequery effect is on
		 disable(2):the remotequery effect is off"
	::={ topCcmtsTemplateCmRemoteQueryCfgEntry 1 }
	
topCcmtsTemplateCmRemoteQueryCommunity OBJECT-TYPE
	SYNTAX		DisplayString
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"This string is the commnunity-string what
		be used for snmp request connect"
	::={ topCcmtsTemplateCmRemoteQueryCfgEntry 2 }
	
topCcmtsTemplateCmRemoteQueryInterval OBJECT-TYPE
	SYNTAX      INTEGER(5..86400)
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"The interval after one whole polling end
		 and before another polling process start"
	::={ topCcmtsTemplateCmRemoteQueryCfgEntry 3 }
	
topCcmtsTemplateCmRemoteQuerySrcIP OBJECT-TYPE
	SYNTAX		IpAddress
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"The IP address used for snmp connecting.
		The IP must be one of the vaild IPs which Device own 
		Returns 0.0.0.0 if no IP is assigned.
		Write 0.0.0.0 to clear the ip assigned"
	::={ topCcmtsTemplateCmRemoteQueryCfgEntry 4 }	

-------------------	 topCcmtsTemplateCmRemoteQueryCfgTable end  --------------------------------


-------------------	 topCcmtsTemplateDrfChannelBlockTable  start--------------------------------
 topCcmtsTemplateDrfChannelBlockTable OBJECT-TYPE
     SYNTAX      SEQUENCE OF TopCcmtsTemplateDrfChannelBlockEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
        "Template drf channel block table"
     ::= { topCcmtsTemplateObject 13}

topCcmtsTemplateDrfChannelBlockEntry OBJECT-TYPE
     SYNTAX      TopCcmtsTemplateDrfChannelBlockEntry
     MAX-ACCESS  not-accessible
     STATUS      current
     DESCRIPTION
         "Template drf channel block entry."
     INDEX {
             topCcmtsTemplateIndex
           }
     ::= { topCcmtsTemplateDrfChannelBlockTable 1 }

TopCcmtsTemplateDrfChannelBlockEntry ::= SEQUENCE {     
     topCcmtsTemplateDrfChannelBlockCfgNumberChannels      Unsigned32
     }  
     
topCcmtsTemplateDrfChannelBlockCfgNumberChannels OBJECT-TYPE
     SYNTAX      Unsigned32
     MAX-ACCESS  read-write
     STATUS      current
     DESCRIPTION
        "The Template Number of QAM Channels N' to configure for the QAM block. 
     This number corresponds to the number of channels that are configured to 
	 be currently transmitted by the block.The maximum number of channels per 
	 block follows the consideration of maximum number of digital channels in 
	 a headend described in the DRFI specification. As a rule N' is valid if 
	 it is less than or equal to N. 
        In addition N minimal requirements consist of even numbers and 1 (one 
	QAM channel per Block Channel). Odd number of QAM channels per Block Channel
	are optional for implementation.
		A Set to an invalid value or not supported value returns
    Error 'wrongValue'."
	
      ::= { topCcmtsTemplateDrfChannelBlockEntry 1 }         
     
-------------------	 topCcmtsTemplateDrfChannelBlockTable  end--------------------------------  

END
	
	 

