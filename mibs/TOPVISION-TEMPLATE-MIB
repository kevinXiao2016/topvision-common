-------------------	 topCcmtsTemplateLoadBalCfgTable  --------------------------------
topCcmtsTemplateLoadBalCfgTable OBJECT-TYPE
    SYNTAX SEQUENCE OF TopCcmtsTemplateLoadBalCfgEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
		" "
    ::= { topCcmtsTemplateObject 6 } 
topCcmtsTemplateLoadBalCfgEntry OBJECT-TYPE
    SYNTAX TopCcmtsTemplateLoadBalCfgEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
		" "
	INDEX { topCcmtsTemplateIndex }
    ::= { topCcmtsTemplateLoadBalCfgTable 1 }

TopCcmtsTemplateLoadBalCfgEntry     ::= SEQUENCE {
	 topCcmtsTemplateLoadBalConfigEnable INTEGER,
	 topCcmtsTemplateLoadBalConfigInterval Unsigned32,
	 topCcmtsTemplateLoadBalConfigMaxMoves Unsigned32,
	 topCcmtsTemplateLoadBalConfigMethod INTEGER,
	 topCcmtsTemplateLoadBalConfigNumPeriod INTEGER,
	 topCcmtsTemplateLoadBalConfigPeriod Unsigned32,
	 topCcmtsTemplateLoadBalConfigTriggerThresh Unsigned32,
	 topCcmtsTemplateLoadBalConfigDiffThresh Unsigned32,
	 topCcmtsTemplateLoadBalConfigRangingOverrideEnable INTEGER,
     topCcmtsTemplateLoadBalConfigDccInitAtdma INTEGER,
	 topCcmtsTemplateLoadBalConfigDccInitScdma INTEGER,
	 topCcmtsTemplateLoadBalConfigDbcInitAtdma INTEGER,
	 topCcmtsTemplateLoadBalConfigDbcInitScdma INTEGER
	}

topCcmtsTemplateLoadBalConfigEnable OBJECT-TYPE
    SYNTAX INTEGER {
    	enable(1),
    	disable(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"enable(1):the load-balance operation is on at this CCMTS
		 disable(2):the load-balance operation is off at this CCMTS" 
	DEFVAL          { enable }
    ::= { topCcmtsTemplateLoadBalCfgEntry 1 } 
    
topCcmtsTemplateLoadBalConfigInterval OBJECT-TYPE
    SYNTAX Unsigned32 (0..3600)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"Minimum number of seconds that must elapse before moving the same modem 
		to a different channel.The rang is [0~3600]."
	DEFVAL          { 240 }
    ::= { topCcmtsTemplateLoadBalCfgEntry 2 } 
    
topCcmtsTemplateLoadBalConfigMaxMoves OBJECT-TYPE
    SYNTAX Unsigned32 (0..1000)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"Maximum number of modems allowed to be moved per load balancer period.
		 The rang is [0~1000]" 
	DEFVAL          { 16 }
    ::= { topCcmtsTemplateLoadBalCfgEntry 3 }
    
topCcmtsTemplateLoadBalConfigMethod OBJECT-TYPE
    SYNTAX INTEGER {
    	utilization(1),
    	modem(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"The method of load-balance.utilization(1) is based on run-time statistics,
		 modem(2) is based on minimum reserved rate."  
	DEFVAL          { utilization }
    ::= { topCcmtsTemplateLoadBalCfgEntry 4 }

topCcmtsTemplateLoadBalConfigNumPeriod OBJECT-TYPE
    SYNTAX INTEGER {
    	one(1),
    	two(2),
    	four(4)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"Number of periods for calculating the weighted average load. Each period 
		 gets twice the weight of the previous period. Valid values are 1, 2, or 4. 
		 Applies to Balancing Method utilization(1) only.When Balancing Method is 
		 modem(2),the value of this object is same as Balancing Method utilization(1)" 
	DEFVAL          { four }
    ::= { topCcmtsTemplateLoadBalCfgEntry 5 }
    
topCcmtsTemplateLoadBalConfigPeriod OBJECT-TYPE
    SYNTAX Unsigned32 (30..3600)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"Time in seconds between load balancing checks. The rang is [30~3600]." 
	DEFVAL          { 60 }
    ::= { topCcmtsTemplateLoadBalCfgEntry 6 }
    
topCcmtsTemplateLoadBalConfigTriggerThresh OBJECT-TYPE
    SYNTAX Unsigned32 (0..100)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"A channel is considered overloaded when the load is at least this percent
		 of capacity. Below this, no attempt is made to balance its load. Applies to
		 Balancing Method utilization(1) only.The rang is [0~100].When Balancing Method 
		 is modem(2),the value of this object is same as Balancing Method utilization(1)" 
	DEFVAL          { 60 }
    ::= { topCcmtsTemplateLoadBalCfgEntry 7 }
    
topCcmtsTemplateLoadBalConfigDiffThresh OBJECT-TYPE
    SYNTAX Unsigned32 (0..100)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"For channels at or above the overload threshold, only consider less loaded 
		 alternate channels where the load difference is at least this percent. Applies 
		 to Balancing Method utilization(1) only.The rang is [0~100].When Balancing Method 
		 is modem(2),the value of this object is same as Balancing Method utilization(1)"
	DEFVAL          { 10 }
    ::= { topCcmtsTemplateLoadBalCfgEntry 8 }
    
topCcmtsTemplateLoadBalConfigRangingOverrideEnable OBJECT-TYPE
    SYNTAX INTEGER {
    	enable(1),
    	disable(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
		"1=enable, 2=Disable. When enabled, a modem can be directed to 
		 a different channel via RNG-RSP prior to registration" 
	DEFVAL         { enable }
    ::= { topCcmtsTemplateLoadBalCfgEntry 9 }
    
topCcmtsTemplateLoadBalConfigDccInitAtdma OBJECT-TYPE
