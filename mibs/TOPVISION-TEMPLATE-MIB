 topCcmtsTemplateSpectrumGroupChnlTable OBJECT-TYPE
	SYNTAX 		SEQUENCE OF TopCcmtsTemplateSpectrumGroupChnlEntry
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION
		"Template Spectrum Group config table"
	::= { topCcmtsTemplateObject 5 }
	
topCcmtsTemplateSpectrumGroupChnlEntry OBJECT-TYPE
	SYNTAX 		TopCcmtsTemplateSpectrumGroupChnlEntry
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION
		"Template  Spectrum Group entry."
	INDEX { topCcmtsTemplateIndex,
			topCcmtsTemplateSpectrumGroupUpChannelChnlId}
	::= { topCcmtsTemplateSpectrumGroupChnlTable 1}
		
TopCcmtsTemplateSpectrumGroupChnlEntry ::=	SEQUENCE { 
		topCcmtsTemplateSpectrumGroupUpChannelChnlId					INTEGER,    		
		topCcmtsTemplateSpectrumGPChnlGroupId       INTEGER, 
		topCcmtsTemplateSpectrumGPChnlSecondaryProf INTEGER,
		topCcmtsTemplateSpectrumGPChnlTertiaryProf  INTEGER       
        }
	
topCcmtsTemplateSpectrumGroupUpChannelChnlId OBJECT-TYPE
	SYNTAX 		INTEGER 
	MAX-ACCESS 	read-only
	STATUS 		current
	DESCRIPTION
		"Template   upstream channel  ID"
	::= { topCcmtsTemplateSpectrumGroupChnlEntry 1 }   

topCcmtsTemplateSpectrumGPChnlGroupId OBJECT-TYPE
	SYNTAX		INTEGER(1..32)
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"the spectrum group ID applied on this channel."
	DEFVAL{1}
	::={ topCcmtsTemplateSpectrumGroupChnlEntry 2 }  
		 
topCcmtsTemplateSpectrumGPChnlSecondaryProf OBJECT-TYPE
	SYNTAX		INTEGER         
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"channel's secondary modulation profile,when modulation hop is
		promissed,modulation will hop between primary modulaition and secondary,or tertiary.  
		65536 indicate no profile configed"  
	DEFVAL{65536}
	::={ topCcmtsTemplateSpectrumGroupChnlEntry 3 }    
	
topCcmtsTemplateSpectrumGPChnlTertiaryProf OBJECT-TYPE
	SYNTAX		INTEGER
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"channel's tertiary modulation profile,when modulation hop is
		promissed,modulation will hop between primary modulaition and secondary,or tertiary.
		65536 indicate no profile configend"
	DEFVAL{65536}
	::={ topCcmtsTemplateSpectrumGroupChnlEntry 4 }             
