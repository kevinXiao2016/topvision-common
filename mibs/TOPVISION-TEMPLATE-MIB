TOPVISION-TEMPLATE-MIB DEFINITIONS ::= BEGIN 

IMPORTS
    MODULE-IDENTITY,
    OBJECT-TYPE,
    Integer32, 
    Unsigned32
        FROM SNMPv2-SMI 
    DateAndTime,
    MacAddress, 
    RowStatus,
    TruthValue,
    DisplayString
        FROM SNMPv2-TC
    ifIndex
				FROM IF-MIB    
    TenthdB,
    TenthdBmV,
    DocsisUpstreamType,
    DocsEqualizerData,
    DocsisQosVersion
             FROM DOCS-IF-MIB        -- RFC 4546

    topCcmtsMibObject
				FROM TOPVISION-CCMTS-MIB;
		
------------------------------------------------------------------------------
--  topCcmtsTemplateObject
--  added at 2013-7-18
--  author qiuxuezhong
------------------------------------------------------------------------------

topCcmtsTemplateObject	OBJECT IDENTIFIER ::={ topCcmtsMibObject 19 } 

-------------------	   topCcmtsTemplateCfgTable--------------------------------
topCcmtsTemplateCfgTable OBJECT-TYPE
	SYNTAX 		SEQUENCE OF TopCcmtsTemplateCfgEntry
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION
		"Template config table"
	::= { topCcmtsTemplateObject 1 }
	
topCcmtsTemplateCfgEntry OBJECT-TYPE
	SYNTAX 		TopCcmtsTemplateCfgEntry
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION
		"Template config entry."
	INDEX { topCcmtsTemplateIndex}
	::= { topCcmtsTemplateCfgTable 1 }
		
TopCcmtsTemplateCfgEntry ::=	SEQUENCE { 
		topCcmtsTemplateIndex		 	        INTEGER,
		topCcmtsTemplateStatus		        	RowStatus,
		topCcmtsTemplateDescription		        DisplayString
	 }

topCcmtsTemplateIndex OBJECT-TYPE
	SYNTAX 		INTEGER 
	MAX-ACCESS 	read-only
	STATUS 		current
	DESCRIPTION
		"Template  index, write a new index to create a new template" 
	::= { topCcmtsTemplateCfgEntry 1 }   
	
topCcmtsTemplateStatus OBJECT-TYPE
	SYNTAX 		RowStatus 
	MAX-ACCESS 	read-create
	STATUS 		current
	DESCRIPTION
		"Template status"
	::= { topCcmtsTemplateCfgEntry 2 }   

topCcmtsTemplateDescription OBJECT-TYPE
	SYNTAX 		DisplayString (SIZE (1..32)) 
	MAX-ACCESS 	read-create
	STATUS 		current
	DESCRIPTION
		"Template description"
	::= { topCcmtsTemplateCfgEntry 3 }   
	

-------------------	   topCcmtsTemplateCfgTable--------------------------------

-------------------	   topCcmtsTemplateUpChannelCfgTable--------------------------------

topCcmtsTemplateUpChannelCfgTable OBJECT-TYPE
	SYNTAX 		SEQUENCE OF TopCcmtsTemplateUpChannelCfgEntry
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION
		"Template up channel config table"
	::= { topCcmtsTemplateObject 2 }
	
topCcmtsTemplateUpChannelCfgEntry OBJECT-TYPE
	SYNTAX 		TopCcmtsTemplateUpChannelCfgEntry
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION
		"Template  up channel entry."
	INDEX { topCcmtsTemplateIndex,
			topCcmtsTemplateUpChannelId}
	::= { topCcmtsTemplateUpChannelCfgTable 1}
		
TopCcmtsTemplateUpChannelCfgEntry ::=	SEQUENCE { 
		topCcmtsTemplateUpChannelId			INTEGER,          
		topCcmtsTemplateUpChannelExtType     INTEGER,        
		topCcmtsTemplateUpChannelWidth        Integer32, 
		topCcmtsTemplateUpChannelModulationProfile       Unsigned32,
		topCcmtsTemplateUpChannelRangingBackoffStart       Integer32, 
		topCcmtsTemplateUpChannelRangingBackoffEnd         Integer32,
		topCcmtsTemplateUpChannelTxBackoffStart     Integer32,    
		topCcmtsTemplateUpChannelTxBackoffEnd       Integer32,      
		topCcmtsTemplateUpChannelPower              TenthdBmV,   
		topCcmtsTemplateUpChannelType                DocsisUpstreamType,    
		topCcmtsTemplateUpChannelFrequency           Integer32,
		topCcmtsTemplateUpChannelAdminStatus   INTEGER    
    } 
	
topCcmtsTemplateUpChannelId OBJECT-TYPE
	SYNTAX 		INTEGER 
	MAX-ACCESS 	read-only
	STATUS 		current
	DESCRIPTION
		"Template   upstream channel  ID"
	::= { topCcmtsTemplateUpChannelCfgEntry 1 }   

topCcmtsTemplateUpChannelAdminStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                up(1),       -- ready to pass packets
                down(2),
                testing(3)   -- in some test mode
            }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The desired state of the interface.  The testing(3) state
            indicates that no operational packets can be passed.  When a
            managed system initializes, all interfaces start with
            ifAdminStatus in the down(2) state.  As a result of either
            explicit management action or per configuration information
            retained by the managed system, ifAdminStatus is then
            changed to either the up(1) or testing(3) states (or remains
            in the down(2) state)."
    ::= { topCcmtsTemplateUpChannelCfgEntry 2}        
       
topCcmtsTemplateUpChannelExtType OBJECT-TYPE  
    SYNTAX      INTEGER {
		v2(2) ,
		v3(3) ,  
		other(4)
		}
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"The UpStreamChannel Type ,including V2(2),V3(3),other(4)."
	::={ topCcmtsTemplateUpChannelCfgEntry 3}    
	
topCcmtsTemplateUpChannelWidth OBJECT-TYPE
        SYNTAX      Integer32 (0..64000000)
        UNITS       "hertz"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The bandwidth of this upstream interface.  This object
             returns 0 if the interface width is undefined or unknown.
             Minimum permitted interface width is currently 200,000 Hz.
             See the associated conformance object for write conditions
             and limitations."
        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Table 6-5."
        ::= { topCcmtsTemplateUpChannelCfgEntry 4 }

topCcmtsTemplateUpChannelModulationProfile OBJECT-TYPE
        SYNTAX      Unsigned32
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "An entry identical to the docsIfModIndex in the
             docsIfCmtsModulationTable that describes this channel.
             This channel is further instantiated there by a grouping
             of interval usage codes (IUCs) that, together, fully
             describe the channel modulation.  This object returns 0 if
             the docsIfCmtsModulationTable entry does not exist or is
             empty.  See the associated conformance object for write
             conditions and limitations.
             Setting this object returns an 'inconsistentValue'
             error if the following conditions are not satisfied:
             1. All the IUC entries in the selected modulation profile
             MUST have the same value of docsIfCmtsModChannelType.
             2. All of the Modulation parameters in the selected
             modulation profile MUST be consistent with the other
             parameters in this docsIfUpstreamChannelEntry."
        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Table 8-19."
        ::= { topCcmtsTemplateUpChannelCfgEntry 5 }      
 topCcmtsTemplateUpChannelRangingBackoffStart OBJECT-TYPE
        SYNTAX      Integer32 (0..16)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The initial random backoff window to use when retrying
             Ranging Requests.  Expressed as a power of 2.  A value of
             16 at the CMTS indicates that a proprietary adaptive retry
             mechanism is to be used.  See the associated conformance
             object for write conditions and limitations."
        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Sections 8.3.4, and 9.4."
        ::= { topCcmtsTemplateUpChannelCfgEntry 6 }

topCcmtsTemplateUpChannelRangingBackoffEnd OBJECT-TYPE
        SYNTAX      Integer32 (0..16)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The final random backoff window to use when retrying
             Ranging Requests.  Expressed as a power of 2.  A value of
             16 at the CMTS indicates that a proprietary adaptive retry
             mechanism is to be used.  See the associated conformance
             object for write conditions and limitations."
        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Section 8.3.4, and 9.4."
        ::= { topCcmtsTemplateUpChannelCfgEntry 7 }
       
        
  
topCcmtsTemplateUpChannelTxBackoffStart OBJECT-TYPE
        SYNTAX      Integer32 (0..16)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The initial random backoff window to use when retrying
             transmissions.  Expressed as a power of 2.  A value of 16
             at the CMTS indicates that a proprietary adaptive retry
             mechanism is to be used.  See the associated conformance
             object for write conditions and limitations."
        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Section 8.3.4, and 9.4."
        ::= { topCcmtsTemplateUpChannelCfgEntry 8 }

topCcmtsTemplateUpChannelTxBackoffEnd OBJECT-TYPE
        SYNTAX      Integer32 (0..16)
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The final random backoff window to use when retrying
             transmissions.  Expressed as a power of 2.  A value of 16
             at the CMTS indicates that a proprietary adaptive retry
             mechanism is to be used.  See the associated conformance
             object for write conditions and limitations."
        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Section 8.3.4, and 9.4."
        ::= { topCcmtsTemplateUpChannelCfgEntry 9 }         

topCcmtsTemplateUpChannelPower OBJECT-TYPE
     SYNTAX      TenthdBmV
     MAX-ACCESS  read-write
     STATUS      current
     DESCRIPTION
        "topCcmtsTemplateUpChannelPower is the power of the
        expected commanded received signal in the channel,
        referenced to the CMTS input."
     ::= { topCcmtsTemplateUpChannelCfgEntry 10 }
   
topCcmtsTemplateUpChannelType OBJECT-TYPE
        SYNTAX      DocsisUpstreamType
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "Reflects the Upstream channel type.
             This object returns the value of docsIfCmtsModChannelType
             for the modulation profile selected in
             docsIfUpChannelModulationProfile for this row."
        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Section 6.2.1."
	::= { topCcmtsTemplateUpChannelCfgEntry 11 }   
	
topCcmtsTemplateUpChannelFrequency OBJECT-TYPE
        SYNTAX      Integer32 (0..1000000000)
        UNITS       "hertz"
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The center of the frequency band associated with this
             upstream interface.  This object returns 0 if the frequency
             is undefined or unknown.  Minimum permitted upstream
             frequency is 5,000,000 Hz for current technology.  See
             the associated conformance object for write conditions
             and limitations."
        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Table 4-2."
        ::= { topCcmtsTemplateUpChannelCfgEntry 12 }

-------------------	   topCcmtsTemplateUpChannelCfgTable--------------------------------
-------------------	   topCcmtsTemplateDownChannelCfgTable--------------------------------
topCcmtsTemplateDownChannelCfgTable OBJECT-TYPE
	SYNTAX 		SEQUENCE OF TopCcmtsTemplateDownChannelCfgEntry
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION
		"Template down channel config table"
	::= { topCcmtsTemplateObject 3 }
	
topCcmtsTemplateDownChannelCfgEntry OBJECT-TYPE
	SYNTAX 		TopCcmtsTemplateDownChannelCfgEntry
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION
		"Template  data entry."
	INDEX { topCcmtsTemplateIndex,
			topCcmtsTemplateDownChannelId}
	::= { topCcmtsTemplateDownChannelCfgTable 1}
		
TopCcmtsTemplateDownChannelCfgEntry ::=	SEQUENCE {    
		topCcmtsTemplateDownChannelId			Integer32, 
		topCcmtsTemplateDownChannelAdminStatus   INTEGER,		
		topCcmtsTemplateDownChannelFrequency        Integer32,
        topCcmtsTemplateDownChannelWidth            Integer32,
        topCcmtsTemplateDownChannelModulation       INTEGER,
        topCcmtsTemplateDownChannelInterleave       INTEGER,
        topCcmtsTemplateDownChannelPower            TenthdBmV,
        topCcmtsTemplateDownChannelAnnex            INTEGER,  
		topCcmtsTemplateDownChannelIsPriCapableDs   TruthValue
    }
	
 topCcmtsTemplateDownChannelId OBJECT-TYPE
     SYNTAX      Integer32 (0..255)
     MAX-ACCESS  read-only
     STATUS      current
     DESCRIPTION
         "The Cable Modem Termination System identification of the
          downstream channel within this particular MAC interface.
          if the interface is down, the object returns the most
          current value.  If the downstream channel ID is unknown,
          this object returns a value of 0."
     ::= { topCcmtsTemplateDownChannelCfgEntry 1 }
  
	
topCcmtsTemplateDownChannelAdminStatus OBJECT-TYPE
    SYNTAX  INTEGER {
                up(1),       -- ready to pass packets
                down(2),
                testing(3)   -- in some test mode
            }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The desired state of the interface.  The testing(3) state
            indicates that no operational packets can be passed.  When a
            managed system initializes, all interfaces start with
            ifAdminStatus in the down(2) state.  As a result of either
            explicit management action or per configuration information
            retained by the managed system, ifAdminStatus is then
            changed to either the up(1) or testing(3) states (or remains
            in the down(2) state)."
    ::= { topCcmtsTemplateDownChannelCfgEntry 2}        


   topCcmtsTemplateDownChannelFrequency OBJECT-TYPE
        SYNTAX      Integer32 (0..1000000000)
        UNITS       "hertz"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The center of the downstream frequency associated with
             this channel.  This object will return the current tuner
             frequency.  If a CMTS provides IF output, this object
             will return 0, unless this CMTS is in control of the
             final downstream frequency.  See the associated
             compliance object for a description of valid frequencies
             that may be written to this object."

        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Section 6.3.3."
        ::= { topCcmtsTemplateDownChannelCfgEntry 3 }

   topCcmtsTemplateDownChannelWidth OBJECT-TYPE
        SYNTAX      Integer32 (0..16000000)
        UNITS       "hertz"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The bandwidth of this downstream channel.  Most
             implementations are expected to support a channel width
             of 6 MHz (North America) and/or 8 MHz (Europe).  See the
             associated compliance object for a description of the
             valid channel widths for this object."
        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Table 6-17."
        ::= { topCcmtsTemplateDownChannelCfgEntry 4 }

   topCcmtsTemplateDownChannelModulation OBJECT-TYPE
        SYNTAX      INTEGER {
            unknown(1),
            other(2),
            qam64(3),
            qam256(4)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The modulation type associated with this downstream
             channel.  If the interface is down, this object either
             returns the configured value (CMTS), the most current
             value (CM), or the value of unknown(1).  See the
             associated conformance object for write conditions and
             limitations.  See the reference for specifics on the
             modulation profiles implied by qam64 and qam256."
        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Table 6-17."
        ::= { topCcmtsTemplateDownChannelCfgEntry 5 }

   topCcmtsTemplateDownChannelInterleave OBJECT-TYPE
        SYNTAX      INTEGER {
            unknown(1),
            other(2),
            taps8Increment16(3),
            taps16Increment8(4),
            taps32Increment4(5),
            taps64Increment2(6),
            taps128Increment1(7),
            taps12increment17(8)
        }
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "The Forward Error Correction (FEC) interleaving used
             for this downstream channel.
             Values are defined as follows:
             taps8Increment16(3):   protection 5.9/4.1 usec,
                                    latency .22/.15 msec
             taps16Increment8(4):   protection 12/8.2 usec,
                                    latency .48/.33 msec
             taps32Increment4(5):   protection 24/16 usec,
                                    latency .98/.68 msec
             taps64Increment2(6):   protection 47/33 usec,
                                    latency 2/1.4 msec
             taps128Increment1(7):  protection 95/66 usec,
                                    latency 4/2.8 msec
             taps12increment17(8):  protection 18/14 usec,
                                    latency 0.43/0.32 msec

            The value 'taps12increment17' is supported by EuroDOCSIS
            cable systems only, and the others by DOCSIS cable systems.

             If the interface is down, this object either returns
             the configured value (CMTS), the most current value (CM),
             or the value of unknown(1).
             The value of other(2) is returned if the interleave
             is known but not defined in the above list.
             See the associated conformance object for write
             conditions and limitations.  See the reference for the FEC
             configuration described by the setting of this object."
        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Table 6-15."
        ::= { topCcmtsTemplateDownChannelCfgEntry 6 }

   topCcmtsTemplateDownChannelPower OBJECT-TYPE
        SYNTAX      TenthdBmV
        UNITS       "dBmV"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "At the CMTS, the operational transmit power.  At the CM,
             the received power level.
             If the interface is down, this object either returns
             the configured value (CMTS), the most current value (CM)
             or the value of 0.  See the associated conformance object
             for write conditions and limitations.  See the reference
             for recommended and required power levels."
        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Tables 6-16, 6-17."
        ::= { topCcmtsTemplateDownChannelCfgEntry 7 }

   topCcmtsTemplateDownChannelAnnex OBJECT-TYPE
        SYNTAX      INTEGER {
            unknown(1),
            other(2),
            annexA(3),
            annexB(4),
            annexC(5)
        }
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The value of this object indicates the conformance of
             the implementation to important regional cable standards.
             annexA : Annex A from ITU-T J.83 is used.
                      (equivalent to EN 300 429)
             annexB : Annex B from ITU-T J.83 is used.
             annexC : Annex C from ITU-T J.83 is used."
        REFERENCE
            "Data-Over-Cable Service Interface Specifications: Radio
             Frequency Interface Specification SP-RFIv2.0-I10-051209,
             Sections 6.3.1, and H.3.1."
        ::= { topCcmtsTemplateDownChannelCfgEntry 8 }
		
	topCcmtsTemplateDownChannelIsPriCapableDs OBJECT-TYPE
     SYNTAX      TruthValue
     MAX-ACCESS  read-create
     STATUS      current
     DESCRIPTION
        "If set to 'true', this attribute configures the downstream
        channel as Primary-Capable. If set to 'false', this attribute 
		configures the downstream channel as Non-Primary-Capable."
     ::= { topCcmtsTemplateDownChannelCfgEntry 9 }
 
-------------------	   topCcmtsTemplateDownChannelCfgTable--------------------------------
-------------------	 topCcmtsTemplateFftMonitorCfgTable  --------------------------------

topCcmtsTemplateFftMonitorCfgTable OBJECT-TYPE
	SYNTAX 		SEQUENCE OF TopCcmtsTemplateFftMonitorCfgEntry
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION
		"Template fft config table"
	::= { topCcmtsTemplateObject 4 }
	
topCcmtsTemplateFftMonitorCfgEntry OBJECT-TYPE
	SYNTAX 		TopCcmtsTemplateFftMonitorCfgEntry
	MAX-ACCESS 	not-accessible
	STATUS 		current
	DESCRIPTION
		"Template  fft config entry."
	INDEX { topCcmtsTemplateIndex}
	::= { topCcmtsTemplateFftMonitorCfgTable 1}
		

TopCcmtsTemplateFftMonitorCfgEntry ::=	SEQUENCE {     
  		topCcmtsTemplateFftMonitorTimeInterval			INTEGER,
		topCcmtsTemplateFftMonitorFreqInterval	    	INTEGER,
		topCcmtsTemplateFftMonitorPollingStatus			INTEGER,
		topCcmtsTemplateCompressedFftDataExpiredTime    INTEGER,
		topCcmtsTemplateFftMonitorDataMode			INTEGER
	 }  

topCcmtsTemplateFftMonitorTimeInterval OBJECT-TYPE
	SYNTAX 		INTEGER 
	MAX-ACCESS 	read-write
	STATUS 		current
	DESCRIPTION
		"Config the CCMTS fft data polling
		time interval"
	::= { topCcmtsTemplateFftMonitorCfgEntry 1 }   

topCcmtsTemplateFftMonitorFreqInterval OBJECT-TYPE
	SYNTAX 		INTEGER 
	MAX-ACCESS 	read-write
	STATUS 		current
	DESCRIPTION
		"Config the CCMTS fft data polling
		freq interval, that affect the data
		number of fft."
	::= { topCcmtsTemplateFftMonitorCfgEntry 2 }   
		
topCcmtsTemplateFftMonitorPollingStatus OBJECT-TYPE
	SYNTAX 		INTEGER (0..1)  
	MAX-ACCESS 	read-write
	STATUS 		current
	DESCRIPTION
		"CMC fft data polling status.Disabled-0, enable-1."
	::= { topCcmtsTemplateFftMonitorCfgEntry 3 }   

topCcmtsTemplateCompressedFftDataExpiredTime OBJECT-TYPE
	SYNTAX 		INTEGER (5..86400)  
	MAX-ACCESS 	read-write
	STATUS 		current
	DESCRIPTION
		"Buffered compressed fft data expired time in second.
		Default value is 5s"
	::= { topCcmtsTemplateFftMonitorCfgEntry 4 }
topCcmtsTemplateFftMonitorDataMode OBJECT-TYPE
	SYNTAX 		INTEGER {
						 average(1),
						 peak_hold(2),
						 real_time(3)
						 }  
	MAX-ACCESS 	read-write
	STATUS 		current
	DESCRIPTION
		"Represent FFT data as average(1) value or peak_hold(2) value
		or real_time(3) value. Default value is real_time(3) value"
	DEFVAL				{ real_time }
	::= { topCcmtsTemplateFftMonitorCfgEntry 5 } 
 
