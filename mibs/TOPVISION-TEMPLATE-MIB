	SYNTAX		SEQUENCE OF TopCcmtsTemplateSharedSecretEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"This table is used to config the CCMTS MIC shared-secret."
	::={ topCcmtsTemplateObject 8 }

topCcmtsTemplateSharedSecretEntry OBJECT-TYPE
	SYNTAX		TopCcmtsTemplateSharedSecretEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"The entry of topCcmtsTemplateSharedSecretTable."
	INDEX { topCcmtsTemplateIndex }
	::={ topCcmtsTemplateSharedSecretTable 1 }

TopCcmtsTemplateSharedSecretEntry ::=SEQUENCE {    
	topCcmtsTemplateSharedSecretEnabled			INTEGER,
	topCcmtsTemplateSharedSecretEncrypted		INTEGER,
	topCcmtsTemplateSharedSecretAuthStr			DisplayString,
	topCcmtsTemplateSharedSecretCipherStr       DisplayString
}       

topCcmtsTemplateSharedSecretEnabled OBJECT-TYPE
	SYNTAX		           INTEGER  {
							  enable(1),
							  disable(2)
						   }
	MAX-ACCESS	           read-write
	STATUS		           current
	DESCRIPTION
		"Enable or disable the CCMTS MIC.
		 If enable be set, must set topCcmtsTemplateSharedSecretEncrypted and 
		 topCcmtsTemplateSharedSecretAuthStr.
		 If disable is set, other is not necessary."
	DEFVAL                 {2}
	::={ topCcmtsTemplateSharedSecretEntry 1 }

topCcmtsTemplateSharedSecretEncrypted OBJECT-TYPE
	SYNTAX                 INTEGER {
			                  unencrypted(0),
		                      encrypted(7)
		                   }
	MAX-ACCESS             read-write
	STATUS                 current	
	DESCRIPTION
		"This option identify the topCcmtsTemplateSharedSecretAuthStr is encrypted or not . 
		
		If unencrypted is set, the topCcmtsTemplateSharedSecretAuthStr must be followed.
		
		If encrypted is set, the topCcmtsTemplateSharedSecretCipherStr must be followed. "    
	DEFVAL                 { 0 }
	::={ topCcmtsTemplateSharedSecretEntry 2 } 
	

topCcmtsTemplateSharedSecretAuthStr OBJECT-TYPE
	SYNTAX		           DisplayString (SIZE (0..16))
	MAX-ACCESS             read-write
	STATUS                 current
	DESCRIPTION
		"The MIC authentication string.  
		 
		 It is the plaintext same as the MIC shared-secret in the CM configuration file,
		 and the length is 1 to 16. If the CCMTS MIC is disabled, length 0 is returned.
		 
		 If the value of topCcmtsTemplateSharedSecretEncrypted is 7,this element value is invisible
		 when read.
		 
		 This OID must be followed when topCcmtsTemplateSharedSecretEncrypted is 0. The value changes
		 will cause topCcmtsTemplateSharedSecretCipherStr are modified. "
	::={ topCcmtsTemplateSharedSecretEntry 3 }


topCcmtsTemplateSharedSecretCipherStr OBJECT-TYPE
	SYNTAX		           DisplayString (SIZE (32))
	MAX-ACCESS             read-write
	STATUS                 current
	DESCRIPTION
		"The cipher is encrypted by topCcmtsTemplateSharedSecretAuthStr in hexadecimal. 
		
		The length must be 32 when set.
				
		This OID must be followed when topCcmtsTemplateSharedSecretEncrypted is 0. The value changes
		will cause topCcmtsTemplateSharedSecretAuthStr are modified. "	 

	::={ topCcmtsTemplateSharedSecretEntry 4 }          
